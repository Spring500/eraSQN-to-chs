	RETURN 1

;-------------------------------------------------
;※カウンター行動のフラグON／OFF　CFLAG:9
;CFLAG:9 = 1 に変更するとOFF
;-------------------------------------------------
;CFLAG:9 = 1

DRAWLINE
;-------------------------------------------------
;CFLAG:201のビット値で調教開始時イベントを管理する
;29bitまでは全キャラ共通で、30bit〜は各キャラで自由に使ってよい
; 0bit 初調教
; 1bit 屈服刻印LV1
; 2bit 屈服刻印LV2
; 3bit 屈服刻印LV3
; 4bit 恋慕
; 5bit 淫乱
; 6bit 恋慕＆妊娠中
; 7bit 恋慕＆明日出産
; 8bit 恋慕＆初出産後
; 9bit 相思相愛
;20bit 夢魔化Lv1（夢魔化直後）
;21bit 夢魔化Lv2
;22bit 夢魔化Lv3
;25bit 胡蝶の夢
;26bit 眷属化


;-------------------------------------------------
;初調教時 CFLAG201
;-------------------------------------------------
IF GETBIT(CFLAG:201, 0) == 0
	PRINTFORMW 
	SETBIT CFLAG:201, 0
;-------------------------------------------------
;調教開始時（一回のみ） CFLAG201
;-------------------------------------------------
;反発刻印Lv3
ELSEIF MARK:反発刻印 == 3
	CALL KX1_KOJO2_X3
;胡蝶の夢で入れ替わり中
ELSEIF GETBIT(CFLAG:201, 25) == 0 && COND("胡蝶の夢") == MASTER
	PRINTFORMW 
	SETBIT CFLAG:201, 25
;主人が夢魔に眷族にされた
ELSEIF GETBIT(CFLAG:201, 26) == 0 && COND("眷属", MASTER) == TARGET
	PRINTFORMW 
	SETBIT CFLAG:201, 26
;屈服刻印Lv1
ELSEIF GETBIT(CFLAG:201, 1) == 0 && MARK:屈服刻印 == 1
	PRINTFORMW 
	SETBIT CFLAG:201, 1
;屈服刻印Lv2
ELSEIF GETBIT(CFLAG:201, 2) == 0 && MARK:屈服刻印 == 2
	PRINTFORMW 
	SETBIT CFLAG:201, 2
;屈服刻印Lv3
ELSEIF GETBIT(CFLAG:201, 3) == 0 && MARK:屈服刻印 == 3 && TALENT:恋慕 == 0
	PRINTFORMW 
	SETBIT CFLAG:201, 3
;恋慕
ELSEIF GETBIT(CFLAG:201, 4) == 0 && TALENT:恋慕
	PRINTFORMW 
	SETBIT CFLAG:201, 4
;相思相愛
ELSEIF GETBIT(CFLAG:201, 9) == 0 && TALENT:相思相愛
	PRINTFORMW 
	SETBIT CFLAG:201, 9
;日付イベントはこちら
ELSEIF CDAYEV()
	CALL KX1_KOJO2_X3
;助手の有無をチェック（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI <= 0
	CALL KX1_KOJO2_X3

;-------------------------------------------------
;助手との掛け合い(YYを助手のキャラ番号に置換、ＸＸを助手のキャラ番号に対応した種族名、X1をキャラ番号に変更する) 
;掛け合いをするキャラを増やす場合は、コピー＆ペーストをして増やす
;-------------------------------------------------
;助手
;ELSEIF NO:ASSI == YY
	;初めて
;	IF CEVENT("助手会話：ＸＸ") == 0
		;恋慕＋恋慕時初口上（陥落イベント）が発生済み
;		IF TALENT:恋慕 && GETBIT(CFLAG:201, 4)
;			PRINTFORMW 
		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;		CALL SET_CEVENT, "助手会話：ＸＸ"
	;二回目以降
;	ELSE
		;恋慕
;		IF TALENT:恋慕
;			PRINTFORMW 
		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;	ENDIF
;	RETURN 1
;口上のある助手が居ない場合は、通常の二回目以降の口上へ飛ぶ
ELSE
	CALL KX1_KOJO2_X3
ENDIF

RETURN 1


