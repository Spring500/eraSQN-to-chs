		ELSEIF RAND:2
			PRINTFORMW 「このまま好きにしてぇ%KOJO_K5_HT()%」
		ELSE
			PRINTFORMW 「はー%KOJO_K5_HT()%　はー%KOJO_K5_HT()%」
			PRINTFORMW %CALLNAME:TARGET%は絶頂の余韻に浸っている…
		ENDIF

		DITEMTYPE:TARGET:0 = 0
		SIF INSERT("Ｖ後背位") == 0
			DITEMTYPE:TARGET:3 = 0
		DITEMTYPE:TARGET:5 = 0
		DITEMTYPE:TARGET:6 = 0
		DITEMTYPE:TARGET:10 = 0
		IF ABL:露出癖 >= 3
			DITEMTYPE:TARGET:3 = 0
		ENDIF
		IF ABL:受虐倾向 >= 3
			DITEMTYPE:TARGET:130 = 0
		ENDIF

		DITEMTYPE:TARGET:60 = 0
		DITEMTYPE:TARGET:61 = 0
		DITEMTYPE:TARGET:62 = 0
		DITEMTYPE:TARGET:63 = 0
		DITEMTYPE:TARGET:64 = 0
		DITEMTYPE:TARGET:65 = 0
		DITEMTYPE:TARGET:66 = 0
		DITEMTYPE:TARGET:67 = 0
		DITEMTYPE:TARGET:68 = 0

	ELSE
		;今回の体位を許可
		SIF INSERT("Ｖ正常位")
			DITEMTYPE:TARGET:60 = 0
		SIF INSERT("Ｖ後背位")
			DITEMTYPE:TARGET:61 = 0
		SIF INSERT("Ｖ屈曲位")
			DITEMTYPE:TARGET:62 = 0
		SIF INSERT("Ｖ対面座位")
			DITEMTYPE:TARGET:63 = 0
		SIF INSERT("Ｖ背面座位")
			DITEMTYPE:TARGET:64 = 0
		SIF INSERT("Ｖ騎乗位")
			DITEMTYPE:TARGET:65 = 0

		SIF GETBIT(TALENT:喜欢的体位, 0)
			DITEMTYPE:TARGET:60 = 0
