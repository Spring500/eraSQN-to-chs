				IF RAND:2
					PRINTFORMW 「ふふ%KOJO_K5_HT()%　%CNAME("二人称")%、愛してあげる%KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「%CNAME("二人称")%、あたしが愛してあげるわねっ%KOJO_K5_HT()%」
				ENDIF
				NEXTCOM = 65
			ELSE
				IF RAND:2
					PRINTFORMW 「%CNAME("二人称", "甘え", "かな")%…あたし、下から突き上げてほしいな…」
					PRINTFORMW 「二人で一緒に動いたら、とっても気持ちいいと思うの…%KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「%CNAME("二人称")%…あたし、%CNAME("二人称")%に気持ちよくなってほしいの…%KOJO_K5_HT()%」
				ENDIF
			ENDIF
			DITEMTYPE:TARGET:65 = 0
		ELSE
			;ランダム値
			IF CONDS("体位") == "正常位" || CONDS("体位") == "屈曲位"
				LOCAL = RANDN(NUM_STR(CSTR:2, "正常位", "後背位", "屈曲位", "対面座位", "背面座位", "騎乗位", "押し倒し騎乗位") ) + 1
