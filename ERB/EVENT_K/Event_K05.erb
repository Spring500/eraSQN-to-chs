				;マゾっ気Lv3以上
				IF ABL:受虐倾向 >= 3
					IF RAND:2
						PRINTFORMW 「%CNAME("二人称", "甘え", "かな")%、獣みたいに後ろから雄々しく犯してぇ%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「後ろからガンガンに犯して欲しいの…%KOJO_K5_HT()%」
					ENDIF
					IF RAND:2
						PRINTFORMW 「身動きできないまま種付けされるのって、凄く興奮しちゃうの…%KOJO_K5_HT()%」
					ENDIF
				ELSE
					IF RAND:2
						PRINTFORMW 「後ろから抱きしめながら愛して…%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「後ろから突き込んでほしいの…%KOJO_K5_HT()%」
					ENDIF
				ENDIF
				DITEMTYPE:TARGET:61 = 0
			;対面座位おねだり
			CASE IS <= NUM_STR(CSTR:2, "正常位", "後背位", "対面座位")
				IF RAND:2
					PRINTFORMW 「キスしながらして…%KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「抱きしめながらえっちしよ…%KOJO_K5_HT()%」
				ENDIF
				DITEMTYPE:TARGET:63 = 0
			;背面座位おねだり
			CASE IS <= NUM_STR(CSTR:2, "正常位", "後背位", "対面座位", "背面座位")
				IF RAND:2
					PRINTFORMW 「後ろから抱きしめながら愛して%KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「旦那さまの膝に座りながら、いろんなとこ触って愛してほしいな…%KOJO_K5_HT()%」
				ENDIF
				DITEMTYPE:TARGET:64 = 0
			;騎乗位おねだり
			CASE IS <= NUM_STR(CSTR:2, "正常位", "後背位", "対面座位", "背面座位", "騎乗位", "押し倒し騎乗位")
				;押し倒し
				IF TEQUIP:推到
					IF RAND:2
						PRINTFORMW 「ふふ%KOJO_K5_HT()%　%CNAME("二人称")%、愛してあげる%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「%CNAME("二人称")%、あたしが愛してあげるわねっ%KOJO_K5_HT()%」
