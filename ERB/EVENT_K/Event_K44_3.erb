;パラメータ変動後に口上を発動します
;-------------------------------------------------
@KOJO_MESSAGE_PALAMCNG_44_3, ARGS
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSIPLAY
;	RETURN 0
;-------------------------------------------------
;パラメータ変動時のセリフ
;-------------------------------------------------
SELECTCASE ARGS
;-------------------------------------------------
;初めてＶ潤がLV3超えた
;-------------------------------------------------
CASE "Ｖ潤LV3突破"
	;初めて
	IF FIRST_EV("Ｖ潤LV3突破") == 0
		;反発刻印Lv3
		IF MARK:排斥刻印 == 3
			;ローションを使った場合
			IF SELECTCOM == 260
				PRINTFORMW
			;それ以外
			ELSE
				PRINTFORMW
			ENDIF
		;それ以外
		ELSE
			;ローションを使った場合
			IF SELECTCOM == 260
				PRINTFORMW 「これを使うってことは、するんですか？」
			;それ以外
			ELSE
				PRINTFORMW 「こ、こんなに、ここが濡れたの初めてです…」
			ENDIF
		ENDIF
	ENDIF
;-------------------------------------------------
;初めて欲情がLV3超えた
;-------------------------------------------------
CASE "欲情LV3突破"
	IF FIRST_EV("欲情LV3突破") == 0
		;反発刻印Lv3
		IF MARK:排斥刻印 == 3
			;媚薬を使った場合
			IF SELECTCOM == 261
				PRINTFORMW
			;それ以外
			ELSE
				PRINTFORMW
			ENDIF
		;それ以外
		ELSE
			;媚薬を使った場合
			IF SELECTCOM == 261
				PRINTFORMW 「こ、こんなお薬なんかで…♪」
			;それ以外
			ELSE
				PRINTFORMW 「さ、流石は調教師ですね…」
			ENDIF
		ENDIF
	ENDIF
;-------------------------------------------------
;初めて恥情がLV3超えた
;-------------------------------------------------
CASE "恥情LV3突破"
	IF FIRST_EV("恥情LV3突破") == 0
		;反発刻印Lv3
		IF MARK:排斥刻印 == 3
			PRINTFORMW
		;それ以外
		ELSE
			PRINTFORMW 「はぁ、はぁ、%CNAME("二人称")%のいじわる…」
		ENDIF
	ENDIF
;-------------------------------------------------
;初めて恐怖がLV3超えた
;-------------------------------------------------
CASE "恐怖LV3突破"
	IF FIRST_EV("恐怖LV3突破") == 0
		;反発刻印Lv3
