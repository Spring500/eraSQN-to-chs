;-------------------------------------------------
;EVENTTRAIN関係（X1をキャラ番号に置換）
;調教開始時のセリフ CFLAG 201〜210を使用
;-------------------------------------------------
@EVENTTRAIN
SIF NO:TARGET != 59
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

;-------------------------------------------------
;※カウンター行動のフラグON／OFF　CFLAG:9
;CFLAG:9 = 1 に変更するとOFF
;-------------------------------------------------
;CFLAG:9 = 1

DRAWLINE
;-------------------------------------------------
;初調教時 CFLAG201
;-------------------------------------------------
IF GETBIT(CFLAG:201, 0) == 0
	DRAWLINE
	PRINTFORMW 部屋に入るなり、%CALLNAME:TARGET%は%CALLNAME:PLAYER%の袖を引っ張ってきた
	PRINTFORMW 「ねえねえ！%CNAME("二人称", "かな")%が%CALLNAME:TARGET%と『むま』するの！？するの！？」
	PRINTFORMW …『夢魔する』とはどういう意味だろう
	PRINTFORMW 「あのね、『むま』するとね、ほわーってしてねっ！ぷあーってしてねっ！すっごいんだよぉ」
	PRINTFORMW なんとなく納得しながら、腕にぶら下がる%CALLNAME:TARGET%をそのまま部屋のベッドに運んだ
	SETBIT CFLAG:201, 0
	RETURN 1

;-------------------------------------------------
;調教開始時（一回のみ） CFLAG201
;-------------------------------------------------
;反発刻印Lv3
ELSEIF MARK:排斥刻印 == 3
	CALL K59_KOJO2_1
;屈服刻印Lv1
ELSEIF GETBIT(CFLAG:201, 1) == 0 && MARK:屈服刻印 == 1
	PRINTFORMW 「%CNAME("二人称", "かな")%！『むま』って『むま』じゃなかったんだね！」
	PRINTFORMW …？
	PRINTFORMW 「%CNAME("二人称", "かな")%としてるのは『むま』じゃなくて『ちょうきょう』なんだよね？」
	PRINTFORMW 「だからぁ、もっと%CALLNAME:TARGET%と『ちょうきょう』しよ♪はやくはやく〜っ」
	PRINTFORMW 微妙にズレた会話に苦笑しながら、%CALLNAME:PLAYER%は%CALLNAME:TARGET%を抱き上げた
SETBIT CFLAG:201, 1
	RETURN 1
;屈服刻印Lv2
ELSEIF GETBIT(CFLAG:201, 2) == 0 && MARK:屈服刻印 == 2
	PRINTFORMW 扉を開けると、そこにいるはずの%CALLNAME:TARGET%が見当たらない
	PRINTFORMW …逃げられたのか！？
	PRINTFORMW 順調に調教が進んでいると思って油断していたのが災いしたか、と唇をかんでいると
	PRINTFORMW 「ていやっ」
	PRINTFORMW 背後に聞こえたかわいらしい掛け声と同時に、急激に視点が落下した
	PRINTFORMW 「えへへーっ、ヒザかっくんせいこーう♪ねえねえ、びっくりしたー？」
	PRINTFORMW 「ねえねぇ…　え、えっとぉ、…怒った？」
	PRINTFORMW …もう少し調教を厳しくせねばなるまい
	SETBIT CFLAG:201, 2
	RETURN 1
;屈服刻印Lv3
ELSEIF GETBIT(CFLAG:201, 3) == 0 && MARK:屈服刻印 == 3 && TALENT:恋慕 == 0
	PRINTFORMW 「あ、%CNAME("二人称", "かな")%っ！きょうもめすぶたにおじひをおねがいしまーすぅ！」
	PRINTFORMW 「えへへー、『どれー』のあいさつだよ！ご本で勉強したの、えらいでしょ」
	PRINTFORMW %CALLNAME:TARGET%は屈託のない笑顔で言うが、意味は分かってないだろう
	PRINTFORMW 「きょうも、あしたも、いーっぱい『ちょうきょう』してねっ！」
	SETBIT CFLAG:201, 3
	RETURN 1
;屈服刻印Lv3+恋慕
ELSEIF GETBIT(CFLAG:201, 4) == 0 && TALENT:恋慕
	PRINTFORMW %CALLNAME:TARGET%は枕を抱き、元気なくベッドに座っていた
	PRINTFORMW 「%CNAME("二人称", "かな", "甘え")%…%CALLNAME:TARGET%、ごびょうきかもしれないの」
	PRINTFORMW 「%CNAME("二人称", "かな")%のことをかんがえるとね、からだが変になるの」
	PRINTFORMW 「あたまがわーってなって、お胸とおなかの下のほうがきゅーってなって、涙がでるの」
	PRINTFORMW 涙目になった%CALLNAME:TARGET%は枕をぎゅっと抱きしめた
	PRINTFORMW 「きもちいいのにきもちわるいの…ねぇ、どーしたら──むぐっ」
	PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%を抱きしめると、唇を奪い舌を絡めた
	PRINTFORMW 「むぅっ、ぷはぁ…ちょっと良くなったかもぉ」
	PRINTFORMW 「もっと治してぇ、%CNAME("二人称", "かな")%じゃなきゃダメなの」
	PRINTFORMW 二人はお互いの唇をむさぼりながらベッドに倒れこんだ
	PRINTFORMW 「ずっと、こうしていたいなぁ♪」
	SETBIT CFLAG:201, 4
	RETURN 1
;助手の有無をチェック（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI <= 0
	CALL K59_KOJO2_1

;口上のある助手が居ない場合は、通常の二回目以降の口上へ飛ぶ
ELSE
	DRAWLINE
	CALL K59_KOJO2_1
ENDIF

