	IF ABL:Ｍ感觉 >= 3
		PRINTFORMW 「んむっ…れろ……じゅる…」
		PRINTFORMW 「『おやすみのちゅっ』てするだけのはずだったのにぃ…ガマンできないよぉ」
	;それ以外
	ELSE
		PRINTFORMW 「%CNAME("二人称", "かな")%、おやすみのちゅーして♪…んっ」
	ENDIF
	RETURN 1
;-------------------------------------------------
;バイブの追加口上 CFLAG377
;-------------------------------------------------
CASE "Ｖバイブ追加口上"
	;バイブの品揃えを見た、夢魔の反応です
	IF TEQUIP:バイブ == 0
		;初めてのバイブ見学
		IF CFLAG:377 == 0
			PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%に、調教用具入れからバイブを持ってくるよう命じた
			PRINTFORMW %CALLNAME:TARGET%は自分で運んでいる銀のトレイに乗った4本のバイブを食い入るように見つめている
			IF ABL:Ｖ感觉 >= 3
				PRINTFORMW %CALLNAME:TARGET%はトレイに並べられたバイブを取っかえ引っかえ手に取っている
				PRINTFORMW 「このちっちゃいのはカワイイね、はじめて見たおちんちんみたい！んー、ほかには…」
			ELSE
				PRINTFORMW %CALLNAME:TARGET%はトレイに並べられたバイブを取っかえ引っかえ手に取っている
				PRINTFORMW 「わ！これ喜んでるワンちゃんのしっぽみたい♪ぶんぶん振ってるぅ」
			ENDIF
			CFLAG:377 = 1
		ELSE
			IF ABL:Ｖ感觉 >= 3
				PRINTFORMW %CALLNAME:TARGET%はトレイに並べられたバイブを取っかえ引っかえ手に取っている
				PRINTFORMW 「すごーい…なんか武器みたい…」「…えーっとぉ…このいちばんおっきいのは……%CALLNAME:TARGET%の脚くらいあるよ…？」
			ELSE
				PRINTFORMW %CALLNAME:TARGET%はトレイに並べられたバイブを取っかえ引っかえ手に取っている
				PRINTFORMW 「このちっちゃいのはカワイイね、はじめて見たおちんちんみたい！んー、ほかには…」「すごーい…なんか武器みたい…」
			ENDIF
		ENDIF
	;後は大きさで分岐
	ELSE
		SELECTCASE SIZE("バイブ")
		;普通
		CASE 1
			;初回の反応
			IF GETBIT(CFLAG:377, 1) == 0
				IF CHECK("バイブ")
					PRINTFORMW 「え…？そのぶんぶん回るの入れちゃうの…？」
				ELSE
					PRINTFORMW 「そんなの入れて、%CALLNAME:TARGET%のオマタこわれちゃわないかなぁ…」
				ENDIF
				SETBIT CFLAG:377, 1
			ELSEIF CHECK("バイブ")
				IF RAND:2
					PRINTFORMW 「おくまでぶるぶるされてるぅ…」
				ELSE
					PRINTFORMW 「ひきゃあぁん！あ、あかちゃんのへやの入り口くるくるされるぅっ♪」
				ENDIF
			ELSEIF RAND:2
				PRINTFORMW 「ば、ばいぶといっしょに%CALLNAME:TARGET%のお腰もぐるぐるしちゃううぅ…」
			ELSE
				PRINTFORMW 「ぶるぶるしながらぁ…はいってきちゃううぅっ…」
			ENDIF
		;太め
		CASE 2
			;初回の反応
			IF GETBIT(CFLAG:377, 2) == 0
				IF CHECK("バイブ")
					PRINTFORMW 「ううぅ…めりめりってぇ…」
				ELSE
					PRINTFORMW 「うわわぁ…オマタのおにく、こんなのびちゃってる…」
				ENDIF
				SETBIT CFLAG:377, 2
			ELSEIF CHECK("バイブ")
				PRINTFORMW 「あ、あかちゃんのへや、こつこつしてるよぉ…」
			ELSE
				PRINTFORMW 「も、もっと…ゆっくりぃ……」
			ENDIF
		;極太以上
		CASE IS >= 3
			;初回の反応
			IF GETBIT(CFLAG:377, 4) == 0
				IF CHECK("バイブ")
					PRINTFORMW 「ふぐううぅっっ！…あ……うそぉ…ホントにはいっちゃった…？」
				ELSE
					PRINTFORMW 「もう奥までキてるのっ！おしこまれても入んないってばぁ〜っ！」
				ENDIF
				SETBIT CFLAG:377, 4
			ELSEIF CHECK("バイブ")
				PRINTFORMW 「お、おなかぼこぉってしてる…んふぅっ、ぼこってなったとこバイブでうごいてるよぉ…」
			ELSE
				PRINTFORMW 「うぐっ…%CALLNAME:TARGET%のオマタ、元にもどんなくなるぅ…」
			ENDIF
		ENDSELECT
	ENDIF
	RETURN 1
;-------------------------------------------------
;アナルバイブの追加口上 CFLAG379
;-------------------------------------------------
CASE "Ａバイブ追加口上"
	;バイブの品揃えを見た、夢魔の反応です
	IF TEQUIP:アナルバイブ == 0
		;初めてのバイブ見学
		IF CFLAG:379 == 0
			IF ABL:Ａ感觉 >= 3
				PRINTFORMW %CALLNAME:TARGET%は並べられたバイブ群の一つを手にとって弄り回している
				PRINTFORMW 偶然スイッチが入ったのか、うねりだしたそれを見て%CALLNAME:TARGET%は驚いている
				PRINTFORMW 「もしかして、これつかって%CALLNAME:TARGET%のオマタであそぶの？……え、おし…え〜〜っ？！」
			ELSE
				PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%に、調教用具入れからアナルバイブを持ってくるよう命じた
				PRINTFORMW %CALLNAME:TARGET%は自分で運んでいる金のトレイに乗ったアナルバイブを赤い顔で見ている
			ENDIF
			CFLAG:379 = 1
		ELSE
			IF ABL:Ａ感觉 >= 3
				PRINTFORMW 「こ、このいちばんおっきなのはジョークだよね？ね？」
			ELSE
				PRINTFORMW 「おしりおもちゃいっぱいだぁ」
			ENDIF
		ENDIF
	;アナルパール
	ELSEIF COND("アナルパール")
		;初回の反応
		IF GETBIT(CFLAG:379, 6) == 0
			PRINTFORMW 「%CALLNAME:TARGET%のおしりが…ちゅぽん…ちゅっぽんってぇ……たべてるぅっ」
			SETBIT CFLAG:379, 6
		ELSEIF RAND:3 == 0
			PRINTFORMW 「んっ、きゅうこぉ…あっ！はぅっ！？……二個抜けたぁ…」
			PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%のアナルに数珠球を数個入れては数個抜いてを繰り返している
			PRINTFORMW %CALLNAME:TARGET%の声が熱くなってきた所で、今何個が腸内にあるかと出題した
			PRINTFORMW 「いっ…いまはぁ…ろっこ、かなぁ……──きゃあぁぁあっ！？いっきに抜いちゃダメっ！？ゃーーーっ！！」
			PRINTFORMW 答えが間違っていたので球を一気に全部引き抜き、最初から入れ直すことにした
		ELSEIF RAND:2
			PRINTFORMW 「い、いっこぉ…に、こおっ！…さんこっ…」
			PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%に入った球の数を申告させている
		ELSE
			PRINTFORMW 「お、おだんご入れられてぇ…ぞくぞくってなる…っ」
		ENDIF
	;後は大きさで分岐
	ELSE
		SELECTCASE SIZE("Ａバイブ")
		;アナルバイブ
		CASE 0
			;初回の反応
			IF GETBIT(CFLAG:379, 2) == 0
				IF CHECK("Ａバイブ")
					PRINTFORMW 「はぅっ…あぅっ…あっ…」
					PRINTFORMW %CALLNAME:TARGET%はアナルバイブが少しづつめり込むたびに小さな声をあげている
				ELSE
					PRINTFORMW 逃げられないように抱えられた%CALLNAME:TARGET%は、アナルにバイブの先端があたるたびに腰を跳ねさせている
				ENDIF
				SETBIT CFLAG:379, 2
			ELSEIF CHECK("Ａバイブ")
				PRINTFORMW 「は、はいったぁ…しっぽ入ったぁ…」
			ELSE
				PRINTFORMW 「うぅ…おしりのあなぴくぴくしてるぅ…」
			ENDIF
		;普通のバイブ
		CASE 1
			;初回の反応
			IF GETBIT(CFLAG:379, 3) == 0
				IF CHECK("Ａバイブ")
					PRINTFORMW 「ん、んとね？%CALLNAME:TARGET%のおしりにそんなのムリだと…おおぉお！？」
				ELSE
					PRINTFORMW %CALLNAME:PLAYER%はヴァギナ用に作られたバイブを手に取った
				ENDIF
				SETBIT CFLAG:379, 3
			ELSEIF CHECK("Ａバイブ")
				PRINTFORMW 「んおぉおおぉんっ！お、お腰まできてるぅうぅ！？」
			ELSE
				PRINTFORMW 「うぎゅうっ！？こんなの入ってたら…あ、あるけなくなっちゃうぅっ」
			ENDIF
		;太いバイブ
		CASE 2
			;初回の反応
			IF GETBIT(CFLAG:379, 4) == 0
				IF CHECK("Ａバイブ")
					PRINTFORMW 「ほ、ほら！%CNAME("二人称", "かな")%見て！？これのカリのとこ、%CALLNAME:TARGET%のグーよりおっきいよ！？」
					PRINTFORMW 「だからムリだって！%CALLNAME:TARGET%こわれちゃ──んぎぃいぃっ！？」
				ELSE
					PRINTFORMW %CALLNAME:PLAYER%は嗜虐的な笑みを浮かべて、%CALLNAME:TARGET%の腕ほどもある大型バイブを手に取った
				ENDIF
				SETBIT CFLAG:379, 4
			ELSEIF CHECK("Ａバイブ")
				PRINTFORMW 「こんあのずっと入れあえへたらああぁ〜〜…ばかになるぅ〜っ♪」
			ELSE
				PRINTFORMW 「く、くるしいのときもちいいのが…わ、かんなくなる…っ」
			ENDIF
		;極太以上のバイブ
		CASE IS >= 3
			;初回の反応
			IF GETBIT(CFLAG:379, 5) == 0
				IF CHECK("Ａバイブ")
					PRINTFORMW 「お、おしりのかべぇ…ぜんぶごしごしされてるぅ…♪」
				ELSE
					PRINTFORMW 「ぐるしいぃ…おなかいだいぃ…」
				ENDIF
				SETBIT CFLAG:379, 5
			ELSE
				IF CHECK("Ａバイブ")
					PRINTFORMW 「%CALLNAME:TARGET%のおしりこわれちゃったぁ…こんなのがきもちくてイくのおっ！」
				ELSEIF RAND:2
					PRINTFORMW 「%CALLNAME:TARGET%のカラダのこーぞーてきに無理だと思うの」
					PRINTFORMW 「え？『入るかじゃない、入れるんだ』って…ちょ！ちょっとまってって！？」
					PRINTFORMW 「ぐぅうっ！裂ける！ぜったい裂けちゃう！あがぁ…両あしの…あいだひろがってるぅ…？！」
				ELSE
					PRINTFORMW 「い…いたいぃ…くるしい……うごけないぃ…」
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
	RETURN 1
ENDSELECT

