;-------------------------------------------------
;ランダムの方が良い傾向の高い口上のランダム化
;-------------------------------------------------
@KOJO_FERATIO(ARG)
#FUNCTIONS
;現状はクララ専用
;ARGは半角文字を１とした時のフェラチオの大体の長さ指定。
;入力されていない場合には、1行分の使用(ARG = 60)とする。
SIF ARG <= 0
	ARG = 60

VARSET LOCALS
FOR LOCAL, 0, 7
	IF LOCAL && RAND:2
		IF RAND:8 == 0
			LOCALS = %LOCALS%ぷふぅ
		ELSEIF RAND:7 == 0
			LOCALS = %LOCALS%はぁ
		ELSEIF RAND:6 == 0
			LOCALS = %LOCALS%うぶ
		ELSEIF RAND:5 == 0
			LOCALS = %LOCALS%んん
		ELSEIF RAND:4 == 0
			LOCALS = %LOCALS%んふぅう
		ELSEIF RAND:3 == 0
			LOCALS = %LOCALS%ごくん
		ELSEIF RAND:2 == 0
			LOCALS = %LOCALS%ぷぁ
		ELSE
			LOCALS = %LOCALS%ぷはぁ
		ENDIF
	ELSEIF RAND:15 == 0
		LOCALS = %LOCALS%ちゅう
	ELSEIF RAND:14 == 0
		LOCALS = %LOCALS%ちゅ
	ELSEIF RAND:13 == 0
		LOCALS = %LOCALS%んん
	ELSEIF RAND:12 == 0
		LOCALS = %LOCALS%んちゅ
	ELSEIF RAND:11 == 0
		LOCALS = %LOCALS%あむ
	ELSEIF RAND:10 == 0
		LOCALS = %LOCALS%ちゅぱ
	ELSEIF RAND:9 == 0
		LOCALS = %LOCALS%んぶ
	ELSEIF RAND:8 == 0
		LOCALS = %LOCALS%ん
	ELSEIF RAND:7 == 0
		LOCALS = %LOCALS%んぐ
	ELSEIF RAND:6 == 0
		LOCALS = %LOCALS%じゅる
	ELSEIF RAND:5 == 0
		IF RAND:3 == 0
			LOCALS = %LOCALS%じゅるるぅ
		ELSEIF RAND:2
			LOCALS = %LOCALS%じゅるるるぅ
		ELSE
			LOCALS = %LOCALS%じゅるるるるぅ
		ENDIF
	ELSEIF RAND:4 == 0
		LOCALS = %LOCALS%ごく
	ELSEIF RAND:3 == 0
		LOCALS = %LOCALS%んむ
	ELSEIF RAND:2 == 0
		LOCALS = %LOCALS%ずちゅ
	ELSE
		LOCALS = %LOCALS%じゅぼ
	ENDIF

	SIF RAND:4
		LOCALS = %LOCALS%っ

	LOCAL:1 = 0
	IF RAND:(1 + LOCAL) || STRLENS(LOCALS) >= ARG
		IF (STRLENS(LOCALS) >= ARG || RAND:3 == 0) && (ABL:奉仕精神 >= 3 || TALENT:恋慕)
			LOCALS = %LOCALS%♪
			LOCAL:1 = 1
		ELSEIF RAND:2
			LOCALS = %LOCALS%！
			LOCAL:1 = 2
		ELSEIF RAND:2
			LOCALS = %LOCALS%……
		ELSEIF STRLENS(LOCALS) < ARG && LOCAL != 6
			LOCALS = %LOCALS%、
		ENDIF
	ENDIF

	SIF STRLENS(LOCALS) >= ARG
		BREAK

	SIF LOCAL:1
		LOCALS = %LOCALS%%BL(1)%
	FOR LOCAL:1, 0, 3
		IF RAND:3 || LOCAL:1 == 0
			LOCALS = %LOCALS%…
		ELSE
			BREAK
		ENDIF
	NEXT
NEXT

RETURNF LOCALS


