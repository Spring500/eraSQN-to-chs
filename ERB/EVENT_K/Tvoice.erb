	SIF SOURCE:快Ｃ && ABL:Ｃ感覚 > 1
		C = 1
	SIF SOURCE:快Ｃ && ABL:Ｃ感覚 > 3
		C = 2
	SIF SOURCE:快Ｖ && ABL:Ｖ感覚 > 1 && C == 0
		C = 1
	SIF SOURCE:快Ｖ && ABL:Ｖ感覚 > 3
		C = 2
	SIF SOURCE:快Ａ && ABL:Ａ感覚 > 1 && C == 0
		C = 1
	SIF SOURCE:快Ａ && ABL:Ａ感覚 > 3
		C = 2
	SIF SOURCE:快Ｂ && ABL:Ｂ感覚 > 1 && C == 0
		C = 1
	SIF SOURCE:快Ｂ && ABL:Ｂ感覚 > 3
		C = 2
	;感じていない口上へ
	SIF C == 0
		CALL VOICE_B
	;感覚4以上だと激しさ+1
	SIF C == 2
		B += 1
	;そして開発済み口上へ
	SIF C > 0
		CALL VOICE_A
ENDIF


;玩具装備・解除系
