	;愛撫・性交系は欲情が大きいと喘ぎ数が+3まで増えます
	;愛撫・性交系
	SELECTCASE SELECTCOM
	;最も軽い　愛撫・胸愛撫・アナル舐め・素股
	CASE 0, 5, 9, 93
		A = 1
		B = 0
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	;軽め　クンニ・ローター・指挿入れ・アナル愛撫・フェラする・泡踊り・自慰
	CASE 1, 30, 7, 2, 4, 94, 3
		A = 2
		B = 1
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	;ちょい重め　Ｇスポット刺激・Ｅマッサージャー
	CASE 8, 32
		A = 3
		B = 2
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	;本番　セックス系全部
	CASE 60 TO 79
		A = 3
		B = 3
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	ENDSELECT
ENDSELECT

;どれくらい感じてるかチェック
