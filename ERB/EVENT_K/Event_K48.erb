;エスト様はいたしている時はあんまりしゃべらないと思うんだ
;かくして淫語とあえぎ声のバランスに悩む、と
;少なくとも屈服刻印LV3以降に使う
@RAND_VOICE_48
A = RAND:4
A += 5

B = 0

REPEAT A
	SIF COUNT == 0
		PRINT 「
	IF COUNT != 0 && COUNT % 2 == 1 && RAND:3 != 0
		PRINT ……
	ELSEIF (COUNT != 0 || COUNT != (A - 1)) && B != COUNT - 1 && COUNT % 2 == 1
		PRINT 、
	ELSEIF RAND:16 == 0
		PRINT はっ
	ELSEIF RAND:15 == 0
		PRINT はあん！　
		B = COUNT
	ELSEIF RAND:14 == 0
		PRINT アン！　
		B = COUNT
	ELSEIF RAND:13 == 0
		PRINT んぅっ
	ELSEIF RAND:12 == 0
		PRINT んあっ
	ELSEIF RAND:11 == 0
		PRINT んっ
	ELSEIF RAND:10 == 0
		PRINT あっあっ
	ELSEIF RAND:9 == 0
		PRINT あはぁ
	ELSEIF RAND:8 == 0
		PRINT あぁ！　
		B = COUNT
	ELSEIF RAND:7 == 0
		PRINT ああぁぁ
	ELSEIF RAND:6 == 0
		PRINT あはぁっ
	ELSEIF RAND:5 == 0
		PRINT ああ
	ELSEIF RAND:4 == 0
		PRINT あっ
	ELSEIF RAND:3 == 0
		PRINT あぁん！　
		B = COUNT
	ELSEIF RAND:2 == 0
		PRINT あぁっ
	ELSE
		PRINT アン！　
		B = COUNT
	ENDIF
	SIF COUNT == (A - 1)
		PRINTL 」
REND
RETURN 1

