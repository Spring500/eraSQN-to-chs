;-------------------------------------------------
@KOJO_MESSAGE_MARKCNG_7_1, ARGS
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSIPLAY
;	RETURN 0

SELECTCASE ARGS
;-------------------------------------------------
;苦痛刻印Lv3取得
;-------------------------------------------------
CASE "苦痛刻印Lv3取得"
	IF FIRST_EV("苦痛刻印Lv3取得") == 0
		PRINTFORMW %CALLNAME:TARGET%はどうやら、怯えているようだ。
		PRINTFORMW %CALLNAME:PLAYER%に何かを訴えかけるような目線を向けている。
		RETURN 1
	ENDIF
;-------------------------------------------------
;快楽刻印Lv3取得
;-------------------------------------------------
CASE "快楽刻印Lv3取得"
	IF FIRST_EV("快楽刻印Lv3取得") == 0
		PRINTFORMW 「アオオオオォォォォッ！！」
		PRINTFORMW %CALLNAME:TARGET%は快楽に溺れつつあるようだ。
		RETURN 1
	ENDIF
;-------------------------------------------------
;屈服刻印Lv3取得
;-------------------------------------------------
CASE "屈服刻印Lv3取得"
	IF FIRST_EV("屈服刻印Lv3取得") == 0
		PRINTFORMW 「わんっ、わんっ、わんっ♪」
		PRINTFORMW %CALLNAME:TARGET%はかなり%CALLNAME:PLAYER%に懐いたようだ、尻尾を振って顔を押し付けてくる。
		RETURN 1
	ENDIF
;-------------------------------------------------
;反発刻印Lv3取得
;-------------------------------------------------
CASE "反発刻印Lv3取得"
	IF FIRST_EV("反発刻印Lv3取得") == 0
		PRINTFORMW 「グルルルルルッ！！」
		PRINTFORMW %CALLNAME:TARGET%は誰が見ても警戒してるとすぐわかる様子で、低く唸っている。
		RETURN 1
	ENDIF
ENDSELECT

