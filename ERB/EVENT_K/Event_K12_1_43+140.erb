;※@COUNTER_KOJO関係（12_1をキャラ番号に置換）
;カウンター行動をトリガーにした口上
;カウンター行動時に口上を発動します
;-------------------------------------------------
@COUNTER_KOJO_12_1, ARGS
SELECTCASE ARGS
CASE "カウンター無し"
	IF NOWEX:絶頂数合計
		PRINTFORMW %CALLNAME:TARGET%は恍惚としている…
	ELSEIF TEQUIP:拘束
		IF GETBIT(TEQUIP:拘束, 40)
			PRINTFORMW %CALLNAME:TARGET%は縄に拘束されて動けない……
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は拘束されて動けない……
		ENDIF
	ELSEIF TEQUIP:触手
		PRINTFORMW %CALLNAME:TARGET%は蠢く触手に翻弄されている……
	ELSEIF TEQUIP:押し倒し
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:PLAYER%の上で笑みを浮かべている……
	ELSE
		CALL COUNTER_TEXT, ARGS
	ENDIF
CASE "Ｖインサート"
	CALL COUNTER_TEXT, ARGS
CASE "Ａインサート"
	CALL COUNTER_TEXT, ARGS
CASE "愛撫"
	CALL COUNTER_TEXT, ARGS
CASE "奉仕"
	CALL COUNTER_TEXT, ARGS
CASE "ＳＭ"
	CALL COUNTER_TEXT, ARGS
CASE "恍惚"
	PRINTFORMW 
CASE "押し倒し"
	PRINTFORMW 「アタシがアンタをイカせてやるわ！」
CASE "ストック使用"
	IF TALENT:恋慕
		PRINTFORMW 「あ、アタシが相手してあげてるんだから当然よねっ」
	ELSE
		PRINTFORMW 「ま、まだできるの…」
	ENDIF
CASE "強制終了"
	PRINTFORMW 
CASE "恍惚から復帰"
	PRINTFORMW 
ENDSELECT

RETURN 1

