;-------------------------------------------------
;調教開始時イベントなどに日付限定イベントがある際に、発生条件を満たしていたらRETURNF 1するようにしてください
;そうするとSHOP画面で〜からお話がありますと表示されるようになります
;また、その日のイベント終了条件があるならRETURNF 0とする（汎用セリフがあるなら常にRETURNF 1 で良い）
;ここの条件をもとにCDAYEV(ARGS, ARG)を用いて調教中の日付イベントの発生を判定することができる
;-------------------------------------------------
@IS_DAYEV_K25_2(ARGS, ARG)
#FUNCTION

SIF ARG == 0 && TARGET
	ARG = TARGET
SIF ARGS == ""
	ARGS = %TEXTS("日付対応イベント")%

;病んでいたらダメ
SIF GETBIT(CFLAG:ARG:201, 30)
	RETURNF 0
;恋慕でないとダメ
SIF TALENT:ARG:恋慕 == 0
	RETURNF 0
;相思相愛でない場合にはダメな時もある
IF TALENT:ARG:相思相爱 == 0
	;寂しがらせていたら一度機嫌を取ってあげないとダメ
	SIF COND("ご無沙汰", ARG) || COND("調教空白期間", ARG) >= 30
		RETURNF 0
ENDIF

SELECTCASE ARGS
CASE "お正月"
	RETURNF 1
CASE "バレンタイン"
	RETURNF 1
CASE "バレンタイン翌日"

CASE "ホワイトデー"
	RETURNF 1
CASE "海の日"
	RETURNF 1
CASE "ハロウィン"
	RETURNF 1
CASE "クリスマス一週間前"
	SIF CEVENT("日付：クリスマス一週間前") == 0
		RETURNF 1
CASE "クリスマス"
	RETURNF 1
CASE "クリスマス翌日"
	;訪れた？すっぽかした？
	SIF DAYEV("クリスマス翌日", "現実のみ")
		RETURNF 1
	;クリスマスに訪れている
	SIF CEVENT("日付：クリスマス", ARG)
		RETURNF 1
CASE "クリスマス期間"

ENDSELECT

RETURNF 0

