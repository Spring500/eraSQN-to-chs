LOCAL:0 = 0
WHILE LOCALS:(LOCAL:0) != ""
	LOCAL:1 = TOINT(LOCALS:(LOCAL:0))
	SIF LOCAL:1 != 0
		ABL:(LOCAL:1) = ABL:(ARG:0):(LOCAL:1)
