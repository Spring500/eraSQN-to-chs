;素質
ARGS:0 = 3,40:41,42:43,50:51,53,55,60,64:61:62,65,66:67,70:71,72,73,80,81,82,83,88,90,91,92,96:97,98,99:100,101:102,103:104,105:106,107:108,109:110,111:112,113,114,117,118,121,124,125,126,127,128,129,130,131,132,133:58,150,151,152,153,154,155,156,157,158,159,160,161,162,163,164,165,180,181,182,183,184,185,190,191,192,193,200,201,202,203
;能力
ARGS:1 = 2,8,15

; 素質のコピー
VARSET LOCALS
SPLIT ARGS:0, ",", LOCALS
LOCAL:0 = 0
WHILE LOCALS:(LOCAL:0) != ""
	LOCAL:10 = 0
	LOCAL:1 = 0
	DO
		LOCAL:2 = STRFIND(LOCALS:(LOCAL:0), ":", LOCAL:1)
		LOCAL:(11+LOCAL:10) = TOINT(SUBSTRING(LOCALS:(LOCAL:0),LOCAL:1,LOCAL:2-LOCAL:1))
		LOCAL:1 = LOCAL:2 + 1
		LOCAL:10 += 1
 	LOOP LOCAL:1 != 0

	LOCAL:2 = 1
	FOR LOCAL:1, 0, LOCAL:10
		IF (TALENT:(LOCAL:(11+LOCAL:1)) != 0)
			LOCAL:2 = 0
			BREAK
		ENDIF
	NEXT

	IF LOCAL:2
		FOR LOCAL:1, 0, LOCAL:10
			TALENT:(LOCAL:(11+LOCAL:1)) = TALENT:(ARG:0):(LOCAL:(11+LOCAL:1))
		NEXT
	ENDIF
	LOCAL:0 += 1
WEND

