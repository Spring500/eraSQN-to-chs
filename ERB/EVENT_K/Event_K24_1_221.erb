;-------------------------------------------------
;髪、顔、股間、腹、尻へのぶっかけ
;-------------------------------------------------
IF SHOOT("ぶっかけ")
	;ウルズグン大量射精
	IF COND("绝顶", PLAYER) == 3
		PRINTFORMW 
	;大量射精
	ELSEIF COND("绝顶", PLAYER) == 2
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;魔法瓶へ転移
;-------------------------------------------------
ELSEIF SHOOT("魔法瓶")
	PRINTFORMW 
;以下は通常の射精区分。
;-------------------------------------------------
;素股、足コキ、スク水や身体で射精させた
;-------------------------------------------------
ELSEIF SHOOT("脚")
	;初めて
	IF FIRST_EV("絶頂：脚") == 0
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ELSEIF SHOOT("素股")
	PRINTFORMW 
;-------------------------------------------------
;口で射精させた
;-------------------------------------------------
ELSEIF SHOOT("口")
	;反発刻印Lv3
	IF MARK:排斥刻印 == 3
		PRINTFORMW 
	;初めて
	ELSEIF FIRST_EV("絶頂：口") == 0
		PRINTFORMW 
	;精液中毒Lv5
	ELSEIF ABL:精液上瘾 >= 5
		PRINTFORMW 
	;精液中毒Lv3
	ELSEIF ABL:精液上瘾 >= 3
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:侍奉精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;胸で射精させた
;-------------------------------------------------
