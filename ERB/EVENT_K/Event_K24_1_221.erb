	;反発刻印Lv3
	IF MARK:排斥刻印 == 3
		PRINTFORMW 
	;初めて
	ELSEIF FIRST_EV("絶頂：胸") == 0
		PRINTFORMW 
	;奉仕精神Lv5
	ELSEIF ABL:侍奉精神 >= 5
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:侍奉精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
