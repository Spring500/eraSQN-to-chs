ELSEIF SHOOT("調教者がオンナ")
	;強絶頂
	IF COND("绝顶", PLAYER) >= 2
		PRINTFORMW 
	;絶頂
	ELSEIF COND("绝顶", PLAYER) == 1
		PRINTFORMW 
	ENDIF
ENDIF

