@COM_ABLE99
;シックスナイン実行判定
SIF COMABLE_DEF(99)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:シックスナイン
	RETURN 1
CALL COM_ABLE91
IF RESULT == 0
	CALL COM_ABLE98
	SIF RESULT == 0
		RETURN 0
ENDIF
CALL COM_ABLE1
IF RESULT == 0
	CALL COM_ABLE4
	SIF RESULT == 0
		RETURN 0
ENDIF
