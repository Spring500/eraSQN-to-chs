@COM_ABLE91
;フェラチオ実行判定
SIF COMABLE_DEF(91)
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:口枷 == 1
