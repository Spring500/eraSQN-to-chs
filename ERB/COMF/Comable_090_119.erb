@COM_ABLE92
;パイズリ実行判定
SIF COMABLE_DEF(92)
	RETURN 0
;胸を剥き出しにできるor胸露出でないとダメ
SIF CHECK_CLO("谷間露出") + CHECK_CLO("はだけ可") == 0
	RETURN 0
;オトコだとダメ
SIF TALENT:オトコ
	RETURN 0
;[巨乳]でなければ技巧2以上、[貧乳]だと技巧3以上必要
SIF (TALENT:貧乳 && ABL:技巧 < 3) || (TALENT:巨乳 == 0 && TALENT:貧乳 == 0 && ABL:技巧 < 2)
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;実行値フィルタ
IF CONFIG("実行値フィルタ") || FLAG:10
	CALL COM_ORDER_COM92, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

@COM_ABLE93
;素股実行判定
SIF COMABLE_DEF(93)
	RETURN 0
;アソコ剥き出しにできないとダメ
SIF !CHECK_CLO("Ｖ露出可")
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;オトコだとダメ
SIF TALENT:オトコ
	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:バイブ
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:お風呂プレイ
	SIF ITEM:プレイマット == 0 && NOITEM == 0
		RETURN 0
ENDIF
;実行値フィルタ
IF CONFIG("実行値フィルタ") || FLAG:10
	CALL COM_ORDER_COM93, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

@COM_ABLE94
;泡踊り実行判定
SIF COMABLE_DEF(94)
	RETURN 0
;ハダカでないとダメ
SIF !CHECK_CLO("ハダカ")
	RETURN 0
;お風呂場プレイ中でプレイマットが無いとダメ
SIF TEQUIP:お風呂プレイ == 0
	RETURN 0
IF TEQUIP:お風呂プレイ
	SIF ITEM:プレイマット == 0 && NOITEM == 0
		RETURN 0
ENDIF
;Ｗ押し倒し中ならオンナでないとダメ
SIF TEQUIP:Ｗ押し倒し && TALENT:オトコ
	RETURN 0
;シャワー中はダメ
SIF TEQUIP:シャワー
	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:バイブ
	RETURN 0
;アナルバイブ挿入中はダメ
SIF TEQUIP:アナルバイブ
	RETURN 0
;クリキャップ使用中は不可
SIF TEQUIP:クリキャップ
	RETURN 0
;クリップローター使用中は不可
SIF TEQUIP:ニプルキャップ
	RETURN 0
;オナホール装着中はダメ
SIF TEQUIP:オナホール
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:口枷
	RETURN 0
;アイマスク使用中は不可
SIF TEQUIP:アイマスク
	RETURN 0
;浣腸使用中は不可
SIF TEQUIP:浣腸
	RETURN 0
;実行値フィルタ
IF CONFIG("実行値フィルタ") || FLAG:10
	CALL COM_ORDER_COM94, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

