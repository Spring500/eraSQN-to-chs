@COM_ABLE90
;手淫実行判定
SIF COMABLE_DEF(90)
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;実行値フィルタ
IF CONFIG("実行値フィルタ") || FLAG:10
	CALL COM_ORDER_COM90, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
