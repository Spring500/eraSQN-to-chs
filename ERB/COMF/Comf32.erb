@COM32
;Ｅマッサージャ
;C感覚が高いとローターの上位置換、低いと拷問器具に…

PRINTL Ｅマッサージャ

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = Ｅマッサージャ

RETURN 1

@FLAG_COM32, ARG
CALL FLAG_COM30, 150

@SOURCE_COM32, ARG
;快Ｃはローターの1.5倍
CALL SOURCE_COM30, 150
VARSET LOCAL

LOCAL:20 += 50
LOCAL:21 += 80

LOCAL:14 = 350

;ABL:Ｃ感觉が低いと痛い
SELECTCASE ABL:Ｃ感觉
CASE 0
	LOCAL:6 = 1000
CASE 1
	LOCAL:6 = 800
CASE 2
	LOCAL:6 = 500
CASE 3
	LOCAL:6 = 300
CASE 4
	LOCAL:6 = 100
CASEELSE
	LOCAL:6 = 0
ENDSELECT

;ぱんつ越し
SIF CHECK_CLO("Ｖ露出") == 0
	TIMES LOCAL:6 , 0.20

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM32
;シックスナイン
SIF TEQUIP:６９式
	PRINTFORM %TEXTS("６９")%
PRINTFORMW %CALLNAME:TARGET%の股間に振動しているＥマッサージャを押しつけた…
