@COM138
;浴尿させる
;直前にフェラ系コマンドなら口内放尿、セックス系なら膣内放尿、Ａセックス系なら温泉浣腸となる
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM138, ARG
;尿の汚れ発生
IF PENIS(PLAYER)
	STAIN:PLAYER:Ｐ = SET_STAIN(STAIN:PLAYER:Ｐ, "尿")
ELSE
	STAIN:PLAYER:Ｖ = SET_STAIN(STAIN:PLAYER:Ｖ, "尿")
ENDIF
IF CHECK_COM("口内放尿")
	STAIN:Ｍ = SET_STAIN(STAIN:Ｍ, "尿")
	IF PENIS(PLAYER)
		CALL EQ_STAIN, TARGET, PLAYER, 0, 2
	ELSE
		CALL EQ_STAIN, TARGET, PLAYER, 0, 3
	ENDIF
	CALL SETFLAG, "飲尿フラグ", TARGET
;妊娠率減少してみる。まぁファンタジーだしな
ELSEIF CHECK_COM("膣内放尿")
	STAIN:Ｖ = SET_STAIN(STAIN:Ｖ, "尿")
	CALL EQ_STAIN, TARGET, PLAYER, 3, 2
	BASE:胎内精液量 /= 2
	BASE:胎内精子量 /= 2
	CALL SETFLAG, "膣内浴尿フラグ", TARGET
	IF CHECK_COM("子宮内放尿")
		BASE:胎内精液量 /= 5
		BASE:胎内精子量 /= 5
		CALL SETFLAG, "子宮内浴尿フラグ", TARGET
	ENDIF
ELSEIF CHECK_COM("温泉浣腸")
	STAIN:Ａ = SET_STAIN(STAIN:Ａ, "尿")
	CALL EQ_STAIN, TARGET, PLAYER, 4, 2
	;量は(大量)ぐらい
	TEQUIP:浣腸 |= 1p1 + 1p2 + 1p3 + 1p11
	;精液浣腸フラグ消去
	CLEARBIT TEQUIP:浣腸, 10
	CALL SETFLAG, "温泉浣腸フラグ", TARGET
ELSEIF CHECK_COM("乳内放尿")
	STAIN:Ｂ = SET_STAIN(STAIN:Ｂ, "尿")
	CALL EQ_STAIN, TARGET, PLAYER, 5, 2
	CALL SETFLAG, "乳内浴尿フラグ", TARGET
ELSEIF CHECK_COM("膀胱内放尿")
	IF PENIS(TARGET)
		STAIN:Ｐ = SET_STAIN(STAIN:Ｐ, "尿")
		CALL EQ_STAIN, TARGET, PLAYER, 2, 2
	ELSE
		STAIN:Ｖ = SET_STAIN(STAIN:Ｖ, "尿")
		CALL EQ_STAIN, TARGET, PLAYER, 3, 2
	ENDIF
	CALL SETFLAG, "膀胱内浴尿フラグ", TARGET
	BASE:尿意 += BASE:PLAYER:尿意
ENDIF

;自分から放尿したので、最後まで出し切ります
BASE:PLAYER:尿意 = 0
IF NOWEX:PLAYER:放尿 == 0
	NOWEX:PLAYER:放尿 += 1
	EX:PLAYER:放尿 += 1
ENDIF

@SOURCE_COM138, ARG
VARSET LOCAL

;基本的にはかなりきついコマンドです
LOCAL:20 += 50
LOCAL:21 += 100
;不潔
LOCAL:8 = 3000
;屈従
LOCAL:13 = 1000
;逸脱
LOCAL:14 = 5000

IF CHECK_COM("口内放尿")
	LOCAL:8 += 2000
	IF CHECK("口内放尿")
		LOCAL:13 += 4000
	ELSE
		LOCAL:15 += 3000
	ENDIF
ELSEIF CHECK_COM("子宮内放尿")
	LOCAL:1 = CALCF("Ｖ刺激", 1, 20)
	SIF TALENT:子宫性感
		TIMES LOCAL:1, 2.00
	LOCAL:8 += 7000
	LOCAL:14 += 10000
	IF CHECK("子宮内放尿")
		LOCAL:13 += 7000
	ELSE
		LOCAL:15 += 5000
	ENDIF
ELSEIF CHECK_COM("膣内放尿")
	LOCAL:1 = CALCF("Ｖ刺激", 1, 10)
	IF CHECK("膣内放尿")
		LOCAL:13 += 2000
	ELSE
		LOCAL:15 += 500
	ENDIF
ELSEIF CHECK_COM("温泉浣腸")
	LOCAL:2 = CALCF("Ａ刺激", 1, 10)
	LOCAL:14 += 3000
	IF CHECK("温泉浣腸")
		LOCAL:13 += 2000
	ELSE
		LOCAL:15 += 1000
	ENDIF
	CALL SOURCE_COM136, 50
ELSEIF CHECK_COM("乳内放尿")
	LOCAL:17 = CALCF("Ｂ刺激", 1, 10)
	LOCAL:14 += 5000
	IF CHECK("乳内放尿")
		LOCAL:13 += 3000
	ELSE
		LOCAL:15 += 2000
	ENDIF
ELSEIF CHECK_COM("膀胱内放尿")
	LOCAL:0 = CALCF("尿道刺激", 1, 10)
	LOCAL:14 += 5000
	IF CHECK("膀胱内放尿")
		LOCAL:13 += 3000
	ELSE
		LOCAL:15 += 2000
	ENDIF
ENDIF

;ABL8,マゾっ気で逸脱減少＆屈従ＵＰ
SELECTCASE ABL:受虐倾向
CASE 0
CASE 1
	TIMES LOCAL:13, 1.20
	TIMES LOCAL:14, 0.80
	TIMES LOCAL:15, 0.80
CASE 2
	TIMES LOCAL:13, 1.50
	TIMES LOCAL:14, 0.60
	TIMES LOCAL:15, 0.60
CASE 3
	TIMES LOCAL:13, 2.00
	TIMES LOCAL:14, 0.40
	TIMES LOCAL:15, 0.40
CASE 4
	TIMES LOCAL:13, 2.50
	TIMES LOCAL:14, 0.20
	TIMES LOCAL:15, 0.20
CASEELSE
	TIMES LOCAL:13, 3.00
	TIMES LOCAL:14, 0.05
	TIMES LOCAL:15, 0.05
ENDSELECT

CALL SOURCE_SMPLAY, "浴尿させる", ARG

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM138
PRINTFORM %CALLNAME:PLAYER%は沸き起こってきた尿意に任せて、
IF CHECK_COM("口内放尿")
	IF PENIS(PLAYER)
		IF PREVCOM == 140
			IF RAND:3 == 0 && TALENT:PLAYER:抖Ｓ
				PRINTFORMW 犯したばかりの口を便器に見立てて小便を流し込んだ
			ELSEIF RAND:2
				PRINTFORMW %CALLNAME:TARGET%の喉の奥に精液に続けて小便を流し込んだ
			ELSE
				PRINTFORM 精液を%TEXTR("ご馳走した", "飲ませた", "味わわせた")%ばかりの
