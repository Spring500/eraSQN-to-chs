	IF PREVEX("Ｖ絶頂") && RAND:3
		PRINTFORM 絶頂の余韻
		IF PREVEX("絶頂数合計") >= 3 || PREVEX("Ｖ絶頂") >= 2
			PRINTFORM %TEXTR("に蕩けている", "でだらしない顔を晒している", "にうっとりしている")%
		ELSE
			PRINTFORM %TEXTR("で脱力している", "で甘く吐息をついている", "に身体を震わせている")%
		ENDIF
