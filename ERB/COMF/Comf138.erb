	ELSEIF COND("指の使用", PLAYER) && RAND:2
		PRINTFORM %CALLNAME:TARGET%%TEXTR("の腰をがっちりと抑えながら", "をしっかりと抱きながら")%
	ELSE
		PRINTFORM %TEXTR("精液で汚したばかり", "種付けした直後", "精液をご馳走したばかり")%の
	ENDIF

