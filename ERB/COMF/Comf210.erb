@COM210
;触手ピストン
;触手は侵入した後は好き勝手に蠢いてますが、動きを強力な突き入れで統制する。

PRINTL 触手ピストン

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = 触手ピストン

RETURN 1

@FLAG_COM210, ARG
SIF TEQUIP:按摩棒
	CALL FLAG_COM8, 100
SIF TEQUIP:肛用按摩棒
	CALL FLAG_COM2, 100
SIF TEQUIP:触手口辱
	CALL FLAG_COM140, 100

@SOURCE_COM210, ARG
SIF TEQUIP:按摩棒
	CALL SOURCE_COM8, 100
SIF TEQUIP:肛用按摩棒
	CALL SOURCE_COM2, 100
SIF TEQUIP:触手口辱
	CALL SOURCE_COM140, 100

VARSET LOCAL

LOCAL:20 += 50
LOCAL:21 += 50

LOCAL:9 = 1000
LOCAL:14 = 1000

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM210
IF PREVCOM == SELECTCOM
	PRINTFORMW \@ CONFIG("触手は主人") ? %CALLNAME:MASTER% # 触手 \@は%CALLNAME:TARGET%の身体で快楽を貪り続け\@FLAG:87 ? た # ている \@…
	RETURN 1
ENDIF
PRINTFORM %CALLNAME:TARGET%
VARSET LOCAL
SIF TEQUIP:按摩棒
	LOCAL += 1
SIF TEQUIP:肛用按摩棒
	LOCAL += 1
SIF TEQUIP:触手口辱
	LOCAL += 1
IF TEQUIP:束缚
	IF LOCAL == 3
		PRINT の体のあらゆる穴を貫いている
	ELSEIF LOCAL == 2
		PRINT の身動きの出来ない体を突き上げている
	ELSEIF TEQUIP:按摩棒
		PRINT の膣に入り込んで奥まで達している
	ELSEIF TEQUIP:肛用按摩棒
		PRINT の足がつかなくなるほど奥まで入り込んでいる
	ELSE
		PRINT を一方的に嬲っている
	ENDIF
ELSE
	IF LOCAL == 3
		PRINT の穴という穴を埋めている
	ELSEIF LOCAL == 2
		PRINT を犯している
	ELSEIF TEQUIP:触手口辱
		PRINT の口に侵入している
	ELSEIF TEQUIP:按摩棒
		PRINT の膣で蠢いている
	ELSEIF TEQUIP:肛用按摩棒
		PRINT の直腸でとぐろを巻いている
	ENDIF
ENDIF
PRINT 触手が、
IF TEQUIP:触手口辱 == 2 && RANDIF(2, TEQUIP:按摩棒, TEQUIP:肛用按摩棒)
	PRINT 喉の奥深くまで擦りあげながら
ELSEIF TEQUIP:按摩棒 && RANDIF(2, TEQUIP:肛用按摩棒)
	IF SIZE("按摩棒", TARGET) >= 3 && RAND:2
		PRINT 肉の洞窟を押し広げながら
	ELSE
		PRINT 肉襞を擦りあげながら
	ENDIF
ELSEIF TEQUIP:肛用按摩棒 && RAND:2
	PRINT 内臓を押し上げながら
ENDIF

IF LOCAL >= 2
	IF RAND:2
		PRINT 一斉に
	ELSE
		PRINT 連携して
	ENDIF
ENDIF
IF SIZE("按摩棒", TARGET) >= 3 && RAND:3 == 0
	PRINT 腹部を盛り上げる程の
ELSEIF SIZE("Ａバイブ", TARGET) >= 3 && RAND:3 == 0
	PRINT 、ゾっとするほどストロークの長い
ELSEIF TEQUIP:按摩棒 + TEQUIP:肛用按摩棒 && RAND:3 == 0
	PRINT 身体を揺さぶるような
ELSEIF RAND:2
	PRINT 激しい
ELSE
	PRINT 凶悪な
ENDIF
PRINTFORMW ピストン運動を始めた…

