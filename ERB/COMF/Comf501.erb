	SELECTCOM = 265
	TEQUIP:MASTER:インセンス = 1p5
ELSEIF RESULT == 12 && LOCAL:265 && ITEM:ムスク
	SELECTCOM = 265
	TEQUIP:MASTER:インセンス = 1p6
