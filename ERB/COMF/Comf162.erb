@FLAG_COM162, ARG
CALL EQ_STAIN, ASSI, MASTER, 0, 2
;汚れをなめ取る
CALL RESET_STAIN_EX, "Ｐ", PLAYER

@SOURCE_COM162, ARG
VARSET LOCAL

LOCAL:20 += 0
LOCAL:21 += 30

;とにかく欲情追加
LOCAL:11 = 500

;ABL:欲情をみる
SELECTCASE ABL:欲望
CASE 0
	TIMES LOCAL:11 , 0.25
CASE 1
	TIMES LOCAL:11 , 0.50
CASE 2
	TIMES LOCAL:11 , 0.75
	LOCAL:21 += 10
CASE 3
	TIMES LOCAL:11 , 1.00
	LOCAL:21 += 20
CASE 4
	TIMES LOCAL:11 , 1.25
	LOCAL:21 += 30
CASEELSE
	TIMES LOCAL:11 , 1.50
	LOCAL:21 += 40
ENDSELECT

;ABL:精液中毒を見る
SELECTCASE ABL:精液中毒
CASE 0
	TIMES LOCAL:11 , 0.10
CASE 1
	TIMES LOCAL:11 , 0.40
CASE 2
	TIMES LOCAL:11 , 0.70
CASE 3
	TIMES LOCAL:11 , 1.00
	LOCAL:21 += 10
CASE 4
	TIMES LOCAL:11 , 1.30
	LOCAL:21 += 20
CASEELSE
	TIMES LOCAL:11 , 1.60
	LOCAL:21 += 30
ENDSELECT

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


