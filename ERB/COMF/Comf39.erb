@COM39
;ローター挿入
PRINTL ローター挿入

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = ローター挿入

RETURN 1

@FLAG_COM39, ARG

@SOURCE_COM39, ARG
VARSET LOCAL

LOCAL:20 += 20
LOCAL:21 += 40
;ABL:欲望をみる
LOCAL:11 = ABL:欲望 * 50

;ABL:Ｖ感觉をみる
LOCAL:1 = CALCF("Ｖ刺激", 0, 20)

;PALAM:Ｖ潤をみる
SELECTCASE PALAM:Ｖ潤
CASE IS >= MAX(PALAM_LV(4 - ABL:Ｖ扩张), 100)
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:6 , 0.10
CASE IS >= MAX(PALAM_LV(3 - ABL:Ｖ扩张), 100)
	TIMES LOCAL:1 , 1.20
	TIMES LOCAL:6 , 0.20
CASE IS >= MAX(PALAM_LV(2 - ABL:Ｖ扩张), 100)
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:6 , 0.50
CASE IS >= MAX(PALAM_LV(1 - ABL:Ｖ扩张), 100)
	TIMES LOCAL:1 , 0.40
	LOCAL:6 += 300
CASEELSE
	TIMES LOCAL:1 , 0.10
	LOCAL:6 += 1000
ENDSELECT

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM39
IF TEQUIP:ローター挿入
	TEQUIP:ローター挿入 = 0
	IF TALENT:淫壶 && RAND:2 == 0
		PRINTFORMW %CALLNAME:TARGET%は膣を蠢かせて、手を使うことなく粘液にまみれたローターを体内から押し出した…
	ELSE
		PRINTFORMW %CALLNAME:TARGET%のアソコから延びるコードを引っ張って、震えるローターを取り出した…
	ENDIF
ELSE
	TEQUIP:ローター挿入 = 1
	PRINTFORMW %CALLNAME:TARGET%の膣にローターを呑み込ませて、スイッチを入れた…
ENDIF


@EQUIP_COM19
CALL SOURCE_COM39, 30
CALL FLAG_COM39, 30

;挿入系と組み合わせると、ボーナス(全体のSOURCEに作用)
IF INSERT("Ｖインサート") || TEQUIP:バイブ
	TIMES SOURCE:快Ｖ , 1.10
	SOURCE:痛み += 100
	SIF SELECTCOM == 67
		SOURCE:痛み += 300
	TIMES SOURCE:欲情追加 , 1.10
ENDIF


RETURN 1

@EQUIP_COM19_2
PRINTL ＜ローター挿入中＞

RETURN 1
