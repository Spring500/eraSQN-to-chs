@COM130
;スパンキング
;実際には苦痛があるためもっと体力気力をもっていかれる

PRINTL スパンキング

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = スパンキング

RETURN 1

@FLAG_COM130, ARG
IF INSERT("インサート") && PENIS(PLAYER)
	CALL SETFLAG, "スパンキングセックスフラグ", TARGET
	CALL SETFLAG, "スパンキングセックスフラグ", PLAYER
ENDIF

@SOURCE_COM130, ARG
VARSET LOCAL

LOCAL:20 += 80
LOCAL:21 += 40

;露出
LOCAL:12 = 200
;逸脱
LOCAL:14 = 500

;PALAM:苦痛をみる
SELECTCASE PALAM:苦痛
CASE IS < PALAMLV:1
	LOCAL:6 = 300
CASE IS < PALAMLV:2
	LOCAL:6 = 500
CASE IS < PALAMLV:3
	LOCAL:6 = 800
CASE IS < PALAMLV:4
	LOCAL:6 = 1200
CASE IS >= PALAMLV:4
	LOCAL:6 = 1800
ENDSELECT

CALL SOURCE_SMPLAY, "スパンキング", ARG

LOCAL:13 += MULTIPLY(LOCAL:6 / 2, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)

SIF AGE(TARGET) == 1
	TIMES LOCAL:13, 1.50
;挿入しながら
IF INSERT("インサート")
	TIMES LOCAL:6, 0.50
	TIMES LOCAL:12, 1.50
	TIMES LOCAL:13, 1.50
ENDIF

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@SOURCE_SMPLAY, ARGS, ARG
SIF ABL:受虐倾向 == 0
	RETURN 0
VARSET LOCAL

SELECTCASE ARGS
CASE "アイマスク"
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*100, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*100, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
CASE "スパンキング"
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*200, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*200, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
CASE "拘束", "口枷", "放尿", "剃毛", "精飲"
	SIF ABL:受虐倾向 <= 1
		RETURN 0
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*200, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*200, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
CASE "イラマチオ"
	SIF ABL:受虐倾向 <= 1
		RETURN 0
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*300, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*300, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;ディープスロート中？
	IF TEQUIP:ディープスロート
		TIMES LOCAL:7, 2.00
		TIMES LOCAL:11, 2.00
	ENDIF
CASE "鞭"
	SIF ABL:受虐倾向 <= 1
		RETURN 0
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*300, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*300, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
CASE "浣腸", "拡張バルーン", "浴尿させる", "フィスト", "尿道カテーテル"
	SIF ABL:受虐倾向 <= 2
		RETURN 0
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*400, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*400, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
CASE "Ｗフィスト"
	SIF ABL:受虐倾向 <= 3
		RETURN 0
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*400, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*400, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
CASE "針", "ピアッシング"
	SIF ABL:受虐倾向 <= 4
		RETURN 0
	;中毒充足
	LOCAL:7 = MULTIPLY(ABL:受虐倾向*400, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
	;欲情追加
	LOCAL:11 = MULTIPLY(ABL:受虐倾向*400, 100 + (ABL:PLAYER:施虐倾向 + TALENT:PLAYER:抖Ｓ)*20)
ENDSELECT

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM130
;前回と同じ
IF PREVCOM == SELECTCOM && COND("前Ｔ調教者") == PLAYER
	PRINTFORM %CALLNAME:PLAYER%は
	IF INSERT("インサート")
		IF RAND:3
			PRINTFORM %TEXTS("合体中", PLAYER)%
		ELSE
			PRINTFORM 激しく腰を振りながら、
		ENDIF
	ELSEIF TEQUIP:亲吻
		PRINTFORM %TEXTS("キス継続")%、
		CALL SETFLAG, "追加キスフラグ", TARGET
	ENDIF
	PRINTFORM %CALLNAME:TARGET%のお尻を
	IF ABL:PLAYER:施虐倾向 >= 3 && RAND:2
		PRINTFORM 平手で
	ELSEIF ABL:PLAYER:施虐倾向 && (ABL:PLAYER:施虐倾向 >= 3 || RAND:2)
		PRINTFORM ピシャピシャ
	ELSE
		PRINTFORM ぺちぺち
	ENDIF
	PRINTFORMW 叩き続けた…
ELSE
	PRINTFORM %CALLNAME:PLAYER%は
	IF INSERT("インサート")
		IF RAND:3
			PRINTFORM %TEXTS("合体中", PLAYER)%
		ELSE
			PRINTFORM グイグイと腰を突き入れながら、
		ENDIF
	ELSEIF TEQUIP:亲吻
		PRINTFORM %TEXTS("キス継続")%、
		CALL SETFLAG, "追加キスフラグ", TARGET
	ENDIF
	PRINTFORM %CALLNAME:TARGET%の%TEXTS("お尻：感触")%を平手で
	IF ABL:PLAYER:施虐倾向 >= 3 && RAND:2
		PRINTFORM 強く
	ELSEIF ABL:PLAYER:施虐倾向 && (ABL:PLAYER:施虐倾向 >= 3 || RAND:2)
		PRINTFORM ピシャピシャ
	ELSE
		PRINTFORM ぺちぺちと
	ENDIF
	PRINTFORMW 叩いた…

	IF INSERT("インサート") && PENIS(PLAYER)
		IF INSERT("Ｖインサート")
			LOCALS = 膣
		ELSEIF INSERT("Ａインサート")
			LOCALS = アナル
		ELSEIF INSERT("尿道インサート")
			LOCALS = 尿道
		ENDIF
		IF ABL:受虐倾向 >= 3
			PRINTFORMW %CALLNAME:TARGET%は叩かれるたび嬉しそうに%LOCALS%の締め付けを強くして%CALLNAME:PLAYER%を愉しませている…
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は苦痛が襲うたびに体を硬直させ、%LOCALS%で%CALLNAME:PLAYER%のペニスを強く締め付けている…
		ENDIF
	ELSEIF CHOSECOM(SELECTCOM) == 0
		IF ABL:PLAYER:施虐倾向 >= 3
			PRINTFORM 何度も叩かれている内に%CALLNAME:TARGET%のお尻は赤く腫れ、
			IF ABL:受虐倾向 >= 3 && TALENT:男性 == 0
				PRINTFORM 痛みで発情してしまったのか
				IF CUP:Ｖ潤 >= 500
					PRINTFORMW アソコは愛液でびしょびしょになってしまっている…
				ELSE
					PRINTFORMW 股間は愛液で濡れはじめている…
				ENDIF
			ELSE
				PRINTFORMW かなり痛々しい有様だ…
			ENDIF
		ELSEIF ABL:受虐倾向 >= 3
			PRINTFORMW %CALLNAME:TARGET%は与えられる刺激に物足りなさそうな素振りを見せているものの、お尻を叩かれるのに明らかに興奮しているようだ…
		ENDIF
	ENDIF
ENDIF
