@FLAG_COM95, ARG

@SOURCE_COM95, ARG
VARSET LOCAL

LOCAL:20 += 40
LOCAL:21 += 130

IF PENIS(PLAYER)
	LOCAL:8 = COM_ORDER_STAIN("Ｐ", "")*10 + 60
ELSE
	LOCAL:8 = COM_ORDER_STAIN("Ｖ", "")*10 + 60
ENDIF
LOCAL:11 = CALCF("実行値による欲情追加")
LOCAL:13 = 400
LOCAL:14 = 400


;ABL:侍奉精神をみる
