@COM_ABLE175
;着せ替え実行判定
SIF COMABLE_DEF(175)
	RETURN 0
;恍惚状態だとダメ
SIF STATE("恍惚", PLAYER)
	RETURN 0
;通常状態以外は脱ぐことのみ可能
IF TEQUIP:押し倒し || TEQUIP:拘束 || TEQUIP:館内露出プレイ || TEQUIP:野外プレイ
	SIF CHECK_CLO("ハダカ")
		RETURN 0
ENDIF
;野外プレイor館内露出プレイ中は、露出癖3Lvか従順4Lvが無いとダメ
SIF (TEQUIP:館内露出プレイ || TEQUIP:野外プレイ) && ABL:従順 < 4 && ABL:露出癖 < 3
	RETURN 0
;実行値フィルタ
IF CONFIG("実行値フィルタ") || FLAG:10
	CALL COM_ORDER_COM175, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

