	RETURN 0
;解除はいつでも可能
SIF TEQUIP:お風呂プレイ
	RETURN 1
;対象が動物耳で、従順が3未満だとダメ
SIF TALENT:動物耳 && ABL:従順 < 3
	RETURN 0
;羞恥プレイ中はダメ
SIF TEQUIP:羞恥プレイ
