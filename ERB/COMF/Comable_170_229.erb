@COM_ABLE178
;二人に押し倒されるコマンド実行判定
SIF COMABLE_DEF(178)
	RETURN 0
;MASTER調教中で無いとダメ
SIF ASSIPLAY
	RETURN 0
;気力が無いとダメ
SIF BASE:気力 == 0
	RETURN 0
;てかげん中はダメ
SIF TEQUIP:てかげん
	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:バイブ || TEQUIP:アナルバイブ
	RETURN 0
;ローター挿入中はダメ
SIF TEQUIP:ローター挿入
	RETURN 0
;演奏中はダメ
SIF TEQUIP:演奏中
	RETURN 0
;以下、助手の条件など
IF ASSI
	;妖精だとダメ（調教者がミニマムならＯＫ）
	;SIF TALENT:ASSI:妖精
	;	RETURN 0
	;恋慕、淫乱、誓約、相思相愛状態ならＯＫとする
	IF TALENT:ASSI:恋慕 || TALENT:ASSI:淫乱 || TALENT:ASSI:誓約 || TALENT:ASSI:相思相愛
	;それ以外なら、助手とTARGETが仲良くないとダメ。
	ELSE
		SIF RELATION:(NO:ASSI) < 120
			RETURN 0
	ENDIF
ENDIF
;実行値フィルタ
IF CONFIG("実行値フィルタ") || FLAG:10
	CALL COM_ORDER_COM178, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

