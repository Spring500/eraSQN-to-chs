@COM_ABLE174
;館内露出プレイ
SIF COMABLE_DEF(174)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:館内露出プレイ
	RETURN 1
;従順かマゾっ気が3以上ないとダメ
SIF ABL:従順 <= 2 && ABL:マゾっ気 <= 2
