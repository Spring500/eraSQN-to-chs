;-------------------------------------------------
;170〜199番,〜プレイ等舞台作成コマンド
;-------------------------------------------------
@COM_ABLE170
;ビデオ撮影実行判定
SIF COMABLE_DEF(170)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:ビデオカメラ
	RETURN 1
;押し倒しの時には解除のみ可能
SIF TEQUIP:押し倒し && TEQUIP:ビデオカメラ == 0
	RETURN 0
;一回に付き一本まで
SIF CSTR:81 != ""
	RETURN 0
;カメラとテープのアイテムを持っているか、アイテムなし設定でないとダメ
SIF ( ITEM:ビデオカメラ == 0 || ITEM:ビデオテープ == 0 ) && NOITEM == 0
	RETURN 0
RETURN 1

@COM_ABLE171
;野外プレイ実行判定
SIF COMABLE_DEF(171)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:野外プレイ
	RETURN 1
;アイテム｢首輪｣を持っているか、アイテムなし設定、もしくは屈服刻印3LVか恋慕でないとダメ
SIF ITEM:首輪 == 0 && NOITEM == 0 && TALENT:恋慕 == 0 && MARK:屈服刻印 < 3
	RETURN 0
;従順かマゾっ気が3以上ないとダメ
SIF ABL:従順 <= 2 && ABL:マゾっ気 <= 2
	RETURN 0
;対象か助手が吸血鬼で、時間が昼だとダメ。夜の日傘があるならＯＫ
IF TIME == 0 && ITEM:夜の日傘 == 0
	SIF TALENT:吸血鬼 || (ASSI && TALENT:ASSI:吸血鬼)
		RETURN 0
ENDIF
;浣腸使用中はプラグがなければ不可
SIF TEQUIP:浣腸 && COND("アナル栓") == 0
	RETURN 0
RETURN 1

@COM_ABLE172
;お風呂場プレイ実行判定
