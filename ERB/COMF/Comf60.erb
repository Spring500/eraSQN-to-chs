	IF TALENT:子宫性感
		TIMES LOCAL:1, 1.25
	ELSE
		TIMES LOCAL:1, 0.25
		LOCAL:9 = 500
	ENDIF
CASE "Ｖインサート"
	LOCALS = ペニス
	LOCAL:20 += 50
	LOCAL:21 += 100
	LOCAL:8 = COM_ORDER_STAIN("Ｐ", "Ｖ")*100 + 100
	LOCAL:1 = CALCF("Ｖ刺激", 2)
	SIF STATE("精强术", PLAYER)
		LOCAL:1 = MULTIPLY(LOCAL:1, 100 + STATE("精强术", PLAYER)*10)
	;調教者の腰使いをみる
	LOCAL:1 = SOURCE_EXABL(LOCAL:1, 33, PLAYER)
CASE "双頭バイブプレイ"
	LOCALS = バイブ
	LOCAL:20 += 100
	LOCAL:21 += 100
	LOCAL:14 = 1000
	LOCAL:1 = CALCF("Ｖ刺激", 2)
	;調教者の腰使いをみる
	LOCAL:1 = SOURCE_EXABL(LOCAL:1, 33, PLAYER)
CASE "指挿入れ"
	LOCALS = バイブ
	LOCAL:20 += 30
	LOCAL:21 += 80
	LOCAL:8 = COM_ORDER_STAIN("手", "Ｖ")*100 + 100
	LOCAL:12 = 300
	LOCAL:13 = 200 + ABL:Ｖ感觉*200
	LOCAL:1 = CALCF("Ｖ刺激", 1)
	;Gスポット
	IF CHECK_COM("Ｇスポ責め")
		LOCAL:20 += 20
		LOCAL:21 += 50
		SIF ABL:Ｖ感觉 >= 3
			LOCAL:1 += 500
		LOCAL:14 += 500
		LOCAL:15 += 50
	ENDIF
	;調教者の指使いをみる
	LOCAL:1 = SOURCE_EXABL(LOCAL:1, 31, PLAYER)
CASE "按摩棒"
	LOCALS = バイブ
	;抜いた時
	IF TEQUIP:按摩棒 == 0
		LOCAL:20 += 10
		LOCAL:21 += 30
	ELSE
		LOCAL:20 += 30
		LOCAL:21 += 100
	ENDIF
	LOCAL:14 = 500
	LOCAL:1 = CALCF("Ｖ刺激", 2)
	;調教者の指使いをみる
