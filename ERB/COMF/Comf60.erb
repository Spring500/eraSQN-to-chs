	;調教者の指使いをみる
	LOCAL:1 = SOURCE_EXABL(LOCAL:1, 31, PLAYER)
CASE "Ｖドラ姦"
	LOCALS = ドラゴン
	LOCAL:20 += 500
	LOCAL:21 += 200
	LOCAL:8 = 2000
	LOCAL:12 = 1200
	LOCAL:13 = 5000
	LOCAL:14 = 30000
	LOCAL:1 = CALCF("Ｖ刺激", 3)
CASE "Ａインサート"
	LOCALS = ペニス
	LOCAL:20 += 100
	LOCAL:21 += 150
	LOCAL:8 = COM_ORDER_STAIN("Ｐ", "Ａ")*100 + 800
	LOCAL:13 = 3000
	LOCAL:14 = 6000
	LOCAL:2 = CALCF("Ａ刺激", 2)
	SIF STATE("精强术", PLAYER)
		LOCAL:2 = MULTIPLY(LOCAL:2, 100 + STATE("精强术", PLAYER)*10)
	;調教者の腰使いをみる
	LOCAL:2 = SOURCE_EXABL(LOCAL:2, 33, PLAYER)
CASE "Ａ指挿入れ"
	LOCALS = Ａバイブ
	LOCAL:20 += 20
	LOCAL:21 += 100
	LOCAL:8 = COM_ORDER_STAIN("手", "Ａ")*100 + 100
	LOCAL:12 = 500
	LOCAL:13 = 500
	LOCAL:14 = 1000
	LOCAL:2 = CALCF("Ａ刺激", 1)
	;調教者の指使いをみる
	LOCAL:2 = SOURCE_EXABL(LOCAL:2, 31, PLAYER)
CASE "Ａバイブ"
	LOCALS = Ａバイブ
	IF TEQUIP:肛用按摩棒 == 0
		LOCAL:20 += 10
		LOCAL:21 += 30
	ELSE
		LOCAL:20 += 60
		LOCAL:21 += 150
	ENDIF
	LOCAL:2 = CALCF("Ａ刺激", 2)
	;調教者の指使いをみる
	LOCAL:2 = SOURCE_EXABL(LOCAL:2, 31, PLAYER)
