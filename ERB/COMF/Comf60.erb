	ENDIF
CASE "子宮口責め"
	LOCALS = ペニス
	LOCAL:20 += 50
	LOCAL:21 += 100
	LOCAL:8 = COM_ORDER_STAIN("Ｐ", "Ｖ")*300 + 100
	LOCAL:1 = CALCF("Ｖ刺激", 2)
	SIF STATE("精强术", PLAYER)
		LOCAL:1 = MULTIPLY(LOCAL:1, 100 + STATE("精强术", PLAYER)*10)
	;調教者の腰使いをみる
	LOCAL:1 = SOURCE_EXABL(LOCAL:1, 33, PLAYER)
