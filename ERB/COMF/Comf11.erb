@COM11
;パイズリする
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM11, ARG
;対象のＰ⇔調教者の胸の汚れが移動
CALL EQ_STAIN, TARGET, PLAYER, 2, 5

@SOURCE_COM11, ARG
VARSET LOCAL

LOCAL:20 += 50
LOCAL:21 += 50

LOCAL:3 = 1000
LOCAL:5 = 1000
LOCAL:12 = 220
LOCAL:14 = 50

;オトコかふたなり
IF PENIS(TARGET)
	;ABL:Ｃ感觉をみる
	LOCAL:0 = CALCF("Ｃ刺激", 1)
	LOCAL:0 = SOURCE_EXABL(LOCAL:0, 34, PLAYER)
ENDIF

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM11
IF PREVCOM == SELECTCOM
	IF TEQUIP:推到
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:PLAYER%の%TEXTS("おっぱい", PLAYER)%で存分に楽しんでいる…
	ELSE
		PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%にパイズリ奉仕を続けた…
	ENDIF
ELSE
	IF TEQUIP:推到
		PRINTFORM %CALLNAME:TARGET%は馬乗りになって
		IF RAND:2
			PRINTFORM 自らのペニス
		ELSE
			PRINTFORM %TEXTS("ペニス：視覚", TARGET)%ペニス
		ENDIF
		PRINTFORM を%CALLNAME:PLAYER%の%TEXTS("おっぱい", PLAYER)%で挟むと、
		IF RAND:2
			PRINTFORMW 胸の谷間を犯し始めた…
		ELSE
			PRINTFORMW 腰を動かしてパイズリを楽しみ始めた…
		ENDIF
	;それ以外
	ELSE
		PRINTFORM %CALLNAME:PLAYER%は%TEXTS("おっぱい", PLAYER)%で%CALLNAME:TARGET%の%TEXTS("ペニス：視覚", TARGET)%ペニスを
		IF TALENT:PLAYER:巨乳 && RAND:2
			PRINT 包み込んで
		ELSEIF TALENT:PLAYER:贫乳 == 0 && RAND:2
			PRINT 挟んで
		ENDIF
		PRINTFORMW 擦りあげた…
	ENDIF
ENDIF
