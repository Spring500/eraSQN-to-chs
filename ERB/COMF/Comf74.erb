@COM74
;背面座位アナル
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

@FLAG_COM74, ARG
CALL FLAG_INSERTION, "Ａインサート", ARG, 70
CALL FLAG_COM5, 30
CALL FLAG_COM7, 20

@SOURCE_COM74, ARG
CALL SOURCE_INSERTION, "Ａインサート", ARG, 70
CALL SOURCE_COM5, 30
;アソコ愛撫
CALL SOURCE_COM7, 20

VARSET LOCAL

LOCAL:12 = 1500

;ABL:Ａ感觉をみる
SELECTCASE ABL:Ａ感觉
CASE 0
	LOCAL:3 = 5
CASE 1
	LOCAL:3 = 15
CASE 2
	LOCAL:3 = 50
CASE 3
	LOCAL:3 = 150
CASE 4
	LOCAL:3 = 300
CASEELSE
	LOCAL:3 = 600
ENDSELECT

;PALAM:欲情をみる
SELECTCASE PALAM:欲情
CASE IS < PALAMLV:1
	TIMES LOCAL:3 , 0.30
CASE IS < PALAMLV:2
	TIMES LOCAL:3 , 0.60
CASE IS < PALAMLV:3
	TIMES LOCAL:3 , 1.00
CASE IS < PALAMLV:4
	TIMES LOCAL:3 , 1.50
CASE IS >= PALAMLV:4
	TIMES LOCAL:3 , 1.80
ENDSELECT

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM74
IF CHECK_COM("３Ｐ奉仕")
	CALL TRAIN_MESSAGE_3P
ELSEIF INSERT("Ａインサート") && SELECTCOM == PREVCOM && COND("前Ｔ調教者") == PLAYER
	PRINTFORM %CALLNAME:TARGET%の
	IF TEQUIP:录像带 && RAND:2
		PRINT アソコをビデオカメラに向かって指で押し開きながら、
	ELSE
		PRINT 胸やアソコを弄ったりしながら、
	ENDIF
	SIF (PENIS(PLAYER)) && STATE("ハードスキン", PLAYER)
		PRINT ガチガチのペニスで
	PRINTFORMW 激しくアナルを突き上げた…
;前回と今回の調教者が同じで、抜かないフラグがある場合
ELSEIF INSERT("インサート") && COND("前Ｔ調教者") == PLAYER
	IF INSERT("Ｖインサート")
		PRINTFORM %CALLNAME:TARGET%の身体を背中から抱き寄せ、前の穴を犯していた%NAMES("ペニス", PLAYER)%をアナルに
		SIF TALENT:怀孕 == 0 || TALENT:PLAYER:抖Ｓ
			PRINT 深々と
		PRINTFORMW 呑み込ませた…
	ELSEIF INSERT("正常位") || INSERT("屈曲位")
		PRINTFORMW アナルから抜き去ることなく%CALLNAME:TARGET%の身体を背中から抱き寄せ、身体を揺さぶり始めた…
	ELSEIF INSERT("後背位")
		PRINTFORMW アナルから抜き去ることなく%CALLNAME:TARGET%の身体を背中から抱き寄せ、身体を揺さぶり始めた…
	ELSEIF INSERT("対面座位")
		PRINTFORMW アナルから抜き去ることなく%CALLNAME:TARGET%の身体を背中から抱き寄せ、身体を揺さぶり始めた…
	ELSEIF INSERT("背面座位")
		IF TEQUIP:亲吻
			PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%%TEXTS("キス継続助詞付き")%、背後から抱きしめつつ再び腰を動かし始めた…
			CALL SETFLAG, "追加キスフラグ", TARGET
		ELSE
			PRINTFORMW %CALLNAME:TARGET%を背後から抱きしめ、再び%CALLNAME:TARGET%の身体を揺らし始めた…
		ENDIF
	ELSEIF INSERT("騎乗位")
		PRINTFORMW アナルから抜き去ることなく%CALLNAME:TARGET%を背中から抱き寄せ、胸を揉みしだきながら身体を揺さぶり始めた…
	ENDIF
ELSE
	PRINTFORM %CALLNAME:PLAYER%は
	IF COND("前Ｔから対象のみ変更") && PREVCOM:1 >= 60 && PREVCOM:1 <= 79
		CALL TEXT, "追加地の文：穴から穴へ"
		PRINTFORM %CALLNAME:TARGET%に身体をもたれかけさせながら%TEXTS("アナル")%アナル
	;穴開きでないパンツをはいていて、ずらされていないなら、必ずずらす
	ELSEIF !CHECK_CLO("Ａ露出") && CHECK_CLO("Ａ露出可")
		PRINTFORM %CALLNAME:TARGET%の
		CALL SETFLAG, "ボトムを露出：テキスト有り", TARGET
		PRINTFORM 、身体をもたれかけさせながら%TEXTS("アナル")%アナル
	ELSE
		PRINTFORM %CALLNAME:TARGET%に身体をもたれかけさせながら%TEXTS("アナル")%アナル
	ENDIF

	IF TEQUIP:羞耻Play
		PRINTFORMW にペニスをゆっくりと呑み込ませ、脚を大きく開かせた…
	ELSEIF (TALENT:淫乱 || ABL:露出癖 >= 3) && RAND:2 == 0
		PRINTFORM にペニスをゆっくりと呑み込ませ、
		IF TEQUIP:亲吻
			PRINTFORMW 唇を奪った…
			CALL SETFLAG, "追加キスフラグ", TARGET
		ELSE
			PRINTFORMW 脚を大きく開かせた…
		ENDIF
	ELSEIF TEQUIP:亲吻
		PRINTFORMW を待ち構えていたペニスでゆっくりと貫き、唇を奪った…
		CALL SETFLAG, "追加キスフラグ", TARGET
	ELSE
		PRINTFORMW を待ち構えていたペニスでゆっくりと貫いた…
	ENDIF

	SIF TALENT:怀孕 && RAND:2 == 0
		PRINTFORMW %CALLNAME:PLAYER%がお腹を撫でてあげると、%CALLNAME:TARGET%はくすぐったそうな声をあげた…
ENDIF
