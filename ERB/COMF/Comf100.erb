RETURN 1

@FLAG_COM100, ARG
;対象のＰ⇔調教者のＰの汚れが移動
SIF PENIS(TARGET)
	CALL EQ_STAIN, TARGET, PLAYER, 2, 2

@SOURCE_COM100, ARG
VARSET LOCAL

LOCAL:20 += 40
LOCAL:21 += 130

LOCAL:8 = COM_ORDER_STAIN("Ｐ", "Ｐ")*10 + 60
LOCAL:11 = CALCF("実行値による欲情追加")
LOCAL:13 = 1200
LOCAL:14 = 400

;ABL:侍奉精神をみる
SELECTCASE ABL:侍奉精神
CASE 0
	LOCAL:4 = 200
	LOCAL:5 = 100
CASE 1
	LOCAL:4 = 250
	LOCAL:5 = 180
CASE 2
	LOCAL:4 = 300
	LOCAL:5 = 250
CASE 3
	LOCAL:4 = 350
	LOCAL:5 = 350
CASE 4
	LOCAL:4 = 400
	LOCAL:5 = 500
	TIMES LOCAL:8 , 0.50
CASEELSE
	LOCAL:4 = 450
	LOCAL:5 = 800
	TIMES LOCAL:8 , 0.10
ENDSELECT

;ABL:技巧をみる
SELECTCASE ABL:技巧
CASE 0
	TIMES LOCAL:4 , 0.50
	TIMES LOCAL:5 , 0.50
CASE 1
	TIMES LOCAL:4 , 0.80
	TIMES LOCAL:5 , 0.80
CASE 2
CASE 3
	TIMES LOCAL:4 , 1.20
	TIMES LOCAL:5 , 1.20
CASE 4
	TIMES LOCAL:4 , 1.50
	TIMES LOCAL:5 , 1.50
CASEELSE
	TIMES LOCAL:4 , 2.00
	TIMES LOCAL:5 , 2.00
ENDSELECT

;ABL:Ｃ感觉をみる
LOCAL:0 = CALCF("Ｃ刺激", 2, 70)

;PALAM:Ｖ润
SELECTCASE PALAM:Ｖ润
CASE IS < PALAMLV:1
	TIMES LOCAL:0 , 0.30
	TIMES LOCAL:4 , 0.60
CASE IS < PALAMLV:2
	TIMES LOCAL:0 , 0.60
	TIMES LOCAL:4 , 0.80
CASE IS < PALAMLV:3
	TIMES LOCAL:0 , 1.00
	TIMES LOCAL:4 , 1.00
CASE IS < PALAMLV:4
	TIMES LOCAL:0 , 1.50
	TIMES LOCAL:4 , 1.20
CASE IS < PALAMLV:5
	TIMES LOCAL:0 , 2.00
	TIMES LOCAL:4 , 1.40
CASEELSE
	TIMES LOCAL:0 , 2.50
	TIMES LOCAL:4 , 1.60
ENDSELECT

;ペニスがあると、快Ｃ増加
SIF PENIS(TARGET)
	TIMES LOCAL:0 , 1.50

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM100
;縄
IF TEQUIP:束缚
	PRINTFORMW %CALLNAME:PLAYER%は身動きの取れない%CALLNAME:TARGET%の水抜き穴を、思う存分犯しぬいた…
;ふたなり？
ELSEIF (TALENT:扶她 || TALENT:男性) && RAND:3 > 0
	PRINTFORM %CALLNAME:PLAYER%は水抜き穴に挿入すると、
	IF TALENT:娇小体型 && BASE:绝顶 < 5000
		PRINT 可愛らしい
	ELSEIF BASE:绝顶 >= 5000 && RAND:2 == 0
		PRINT おへそに向かって勃起している
	ELSEIF BASE:绝顶 >= 3000
		PRINT 形が判る位に生地を押し上げている
	ENDIF
	PRINTFORM %CALLNAME:TARGET%のペニス
	IF RAND:2 == 0
		PRINTW ごと、スク水生地で包んで激しく擦り上げた…
	ELSE
		PRINTW にゴシゴシと擦りつけた…
	ENDIF
;説明的
ELSEIF RAND:3 == 0
	PRINTFORMW %CALLNAME:TARGET%を仰向けに寝そべらせると、スク水の下腹部に開いている水抜き穴にペニスを挿入して、スベスベしたお腹とスク水生地の感触を楽しんだ…
ELSEIF RAND:2 == 0
	;「恥じらい」で「臆病」でない
	IF TALENT:羞耻心强 && TALENT:胆怯 == 0
		PRINT 恥ずかしそうに
	;「臆病」で「恋慕」でない
	ELSEIF TALENT:胆怯 && TALENT:恋慕 == 0
		PRINT おずおずと
	;「小悪魔」でランダム1/2
	ELSEIF TALENT:小恶魔 && RAND:2 == 0
		PRINT ニヤニヤしながら
	;淫乱or欲情3000以上
	ELSEIF (TALENT:淫乱 || PALAM:欲情 >= 3000) && RAND:4 > 0
		PRINT 妖艶に微笑みながら
	ELSEIF RAND:2 == 0 && TEQUIP:眼罩 == 0
		SIF RAND:2 == 0
			PRINT 楽しそうに
		PRINT 微笑みながら
	ELSEIF ABL:侍奉精神 >= 3
		PRINT 率先的に
	ENDIF
	PRINTFORMW 水抜き穴を拡げている%CALLNAME:TARGET%に誘われるままに、スク水コキを楽しんだ…
ELSE
	PRINTFORM %CALLNAME:PLAYER%が水抜き穴にペニスを入れると、%CALLNAME:TARGET%は
	;「恥じらい」で「臆病」でない
	IF TALENT:羞耻心强 && TALENT:胆怯 == 0
		PRINT 瞳を伏せながら
	;「臆病」で「恋慕」でない
	ELSEIF TALENT:胆怯 && TALENT:恋慕 == 0
		PRINT おずおずと
	;「小悪魔」でランダム1/2
	ELSEIF TALENT:小恶魔 && RAND:2 == 0
		PRINT ニヤニヤしながら
	;淫乱or欲情3000以上
	ELSEIF (TALENT:淫乱 || PALAM:欲情 >= 3000) && RAND:4 > 0
		PRINT 妖艶に微笑みながら
	ELSEIF RAND:2 == 0 && TEQUIP:眼罩 == 0
		SIF RAND:2 == 0
			PRINT 楽しそうに
		PRINT 微笑みながら
	ELSEIF ABL:侍奉精神 >= 3
		PRINT お腹の上に手を置いて、
	ENDIF
	PRINTFORMW スク水越しにペニスを刺激してきた…
ENDIF


@COM_ORDER_COM100, ARG
VARSET LOCAL
LOCALS = 

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER, ARG

LOCAL = RESULT

;ABL:侍奉精神
IF ABL:侍奉精神
	LOCAL += ABL:侍奉精神 * 4
	LOCALS = %LOCALS% + %ABLNAME:6%LV{ABL:侍奉精神}({ABL:侍奉精神 * 4})
ENDIF
;ABL:精液上瘾
IF ABL:精液上瘾
	LOCAL += ABL:精液上瘾 * 3
	LOCALS = %LOCALS% + %ABLNAME:11%LV{ABL:精液上瘾}({ABL:精液上瘾 * 3})
ENDIF
;恥じらい
IF TALENT:羞耻心强
	LOCAL -= 5
	LOCALS = %LOCALS% - %TALENTNAME:35%(5)
ENDIF
;快感の否定
IF TALENT:否认快感
	LOCAL -= 1
	LOCALS = %LOCALS% - %TALENTNAME:71%(1)
ENDIF

;媚薬
IF TEQUIP:媚药
	LOCAL += 6
	LOCALS = %LOCALS% + %ITEMNAME:31%(6)
ENDIF

;汚れあり
IF COM_ORDER_STAIN("Ｐ", "Ｐ")/3
	LOCAL -= COM_ORDER_STAIN("Ｐ", "Ｐ")/3
	LOCALS = %LOCALS% - 汚れあり
	;汚臭鈍感
	SIF TALENT:污臭钝感 || TALENT:污臭敏感
		LOCALS = %LOCALS%、\@TALENT:污臭钝感 ? %TALENTNAME:61% # %TALENTNAME:62%\@
	LOCALS = %LOCALS%({COM_ORDER_STAIN("Ｐ", "Ｐ")/3})
ENDIF

;合計を表示(30以上で実行)
CALL PRINT_COM_ORDER_COM, LOCALS, ARG, LOCAL, 30

RETURN RESULT
