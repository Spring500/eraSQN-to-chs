@COM36
;搾乳器
IF TEQUIP:触手
	PRINTL 触手搾乳
ELSE
	PRINTL 搾乳器
ENDIF

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

IF TEQUIP:触手
	STR:0 = 触手搾乳
ELSE
	STR:0 = 搾乳器
ENDIF

RETURN 1

@FLAG_COM36, ARG

@SOURCE_COM36, ARG
VARSET LOCAL

LOCAL:20 += 50
LOCAL:21 += 100

LOCAL:4 = 100
LOCAL:5 = 100
LOCAL:6 = 100
LOCAL:7 = 100
LOCAL:12 = 100
LOCAL:13 = 100
LOCAL:16 = 100

;ABL:Ｂ感觉をみる
LOCAL:17 = CALCF("Ｂ刺激", 60)

SIF TALENT:巨乳
	TIMES LOCAL:17 , 1.50
;貧乳
SIF TALENT:贫乳
	TIMES LOCAL:6 , 1.50

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM36
IF TEQUIP:搾乳機
	TEQUIP:搾乳機 = 0
	PRINTFORMW %CALLNAME:TARGET%から搾乳器を取り外した…
ELSE
	TEQUIP:搾乳機 = 1
	PRINTFORMW %CALLNAME:TARGET%に搾乳器を取り付けた…
ENDIF

@TRAIN_MESSAGE_COM205
IF TEQUIP:搾乳機
	TEQUIP:搾乳機 = 0
	PRINTFORMW 触手は%CALLNAME:TARGET%の乳首を解放した…
ELSE
	TEQUIP:搾乳機 = 1
	IF RAND:2 == 0
		PRINTFORMW 吸盤状の触手が%CALLNAME:TARGET%の乳首を吸いあげはじめた…
	ELSE
		;貧乳
		IF TALENT:贫乳 || TALENT:男性
			PRINTFORMW 御椀状の触手が%CALLNAME:TARGET%の胸板に張り付き、リズミカルに吸引を始めた…
		ELSE
			PRINTFORMW 御椀状の触手が%CALLNAME:TARGET%の乳房に張り付き、リズミカルに吸引を始めた…
		ENDIF
	ENDIF
	CALL ADD_STAIN, TARGET, "Ｂ", "触手"
ENDIF


@EQUIP_COM16
;搾乳器装備中
CALL SOURCE_COM36, 30
CALL FLAG_COM36, 30
RETURN 1

@EQUIP_COM16_2
;搾乳器装備中
IF TEQUIP:触手
	PRINTL ＜触手搾乳中＞
ELSE
	PRINTL ＜搾乳器装備中＞
ENDIF

SIF TEQUIP:触手
	CALL SYOKUSYU_MILK

RETURN 1
