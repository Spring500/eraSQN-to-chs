;-------------------------------------------------
;500番台,派生コマンド(コメントアウト解除禁止)
;-------------------------------------------------
@COM_ABLE500
;アルカナ使用実行判定
FOR LOCAL, 230, 260
	TRYCALLFORM COM_ABLE{LOCAL}_2
	SIF RESULT == 1
		RETURN 1
NEXT
RETURN 0

@COM_ABLE501
;アイテム使用実行判定
FOR LOCAL, 260, 280
	TRYCALLFORM COM_ABLE{LOCAL}_2
	SIF RESULT == 1
		RETURN 1
NEXT
RETURN 0

;-------------------------------------------------
;全部の@COMABLEを一斉に管理する時に使用する式中関数
;押し倒しなど、登場頻度が非常に多いものについてのみ扱う
;バイブなどの装備品が解除できるかどうかの判定は、この判定よりも優先度が高いことに注意
;なお、NEXTCOMの時には@COM_ABLEを通らない。
;-------------------------------------------------
@COMABLE_DEF(ARG)
#FUNCTION
;一括OFF、個別設定コマンドフィルタ
SIF FLAG:238 || DITEMTYPE:TARGET:ARG > 0
	RETURNF 1
;コマンド禁止フィルタ
SIF DITEMTYPE:0:ARG == 2 && CONFIG("調教コンフィグモード") == 0
	RETURNF 1

;Ｖ調教フィルタ
IF TALENT:处女 && CONFIG("Ｖ調教フィルタ")
	SELECTCASE ARG
	CASE 304
		SIF TEQUIP:推到 == 0
			RETURNF 1
	CASEELSE
		SIF TRAIN_GENRE("Ｖ調教", ARG)
			RETURNF 1
	ENDSELECT
ENDIF
;助手のコマンドへのＶ調教フィルタ
IF CONFIG("Ｖ調教フィルタ") && (ARG == 160 || ARG == 161)
	SIF ASSI && TALENT:ASSI:处女
		RETURNF 1
ENDIF
;Ａ調教フィルタ(拡張バルーンだけ特殊)
IF CONFIG("Ａ調教フィルタ")
	SIF TRAIN_GENRE("Ａ調教", ARG)
		RETURNF 1
ENDIF

;脱童貞フィルタ
IF CONFIG("脱童貞フィルタ")
	IF TALENT:PLAYER:童贞
		SELECTCASE ARG
		CASE 60 TO 69, 160, 177
			RETURNF 1
		ENDSELECT
	ENDIF
	IF TALENT:童贞
		SELECTCASE ARG
		CASE 120 TO 124, 161
			RETURNF 1
		ENDSELECT
	ENDIF
ENDIF
;運命検閲
IF TEQUIP:命运检阅
	IF COND("運命検閲：愛撫")
		SELECTCASE ARG
		CASE 0, 2, 3, 5, 7
			RETURNF 1
		ENDSELECT
	ENDIF
	IF COND("運命検閲：アソコ")
		SELECTCASE ARG
		CASE 0, 1, 7, 30, 32, 34, 80, 98, 203
			RETURNF 1
		ENDSELECT
	ENDIF
	IF COND("運命検閲：胸")
		SELECTCASE ARG
		CASE 0, 3, 5, 35, 36, 81, 82, 92, 104, 204, 205
			RETURNF 1
		ENDSELECT
	ENDIF
	IF COND("運命検閲：キス")
		SELECTCASE ARG
		CASE 0, 6, 311
			RETURNF 1
		ENDSELECT
	ENDIF
	IF COND("運命検閲：アナル")
		SELECTCASE ARG
		CASE 2, 9, 96
			RETURNF 1
		ENDSELECT
	ENDIF
	IF COND("運命検閲：詠唱")
		SELECTCASE ARG
		CASE 230 TO 259
			RETURNF 1
		ENDSELECT
	ENDIF
ENDIF

;プレイの制限
;受け手が制限されている
