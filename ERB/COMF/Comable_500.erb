	SELECTCASE ARG
	CASE 60 TO 69, 177
		SIF COND("身嗜み：Ｖ独占")
			RETURNF 1
	CASE 70 TO 79
		SIF COND("身嗜み：Ａ独占")
			RETURNF 1
	CASE 90, 96, 98, 99, 140, 150
		SIF COND("身嗜み：口独占")
			RETURNF 1
	CASE 6, 311
		SIF COND("身嗜み：キス独占")
			RETURNF 1
	CASE 120 TO 129
		SIF PENIS(TARGET) && COND("身嗜み：Ｐ独占")
			RETURNF 1
	ENDSELECT
ENDIF
;攻め手が制限されている
