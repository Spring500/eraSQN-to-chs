TCVAR:28 = RESULT

@PRINT_COM_ORDER_COM, ARGS, ARG, ARG:1, ARG:2
IF ARG == 0
	PRINTFORM %ARGS% = {ARG:1}
	SELECTCASE ARG:1
	CASE IS < ARG:2
		PRINT <
	CASE ARG:2
		PRINT =
	CASEELSE
		PRINT >
	ENDSELECT
	PRINTFORMW  実行値{ARG:2}
ENDIF

;RESULT > 0で実行値クリア
RETURN MAX(ARG:1 - ARG:2 + 1, 0)


@COM_ORDER, ARG
VARSET LOCAL
;少しは早くなるか…？
IF TCVAR:27 && ARG
	LOCAL = TCVAR:27
	RETURN LOCAL
ENDIF
LOCALS =
;夢魔
IF TALENT:梦魔 || TALENT:上位梦魔
	LOCAL += 5
	LOCALS = %LOCALS%夢魔(5)
ELSEIF TALENT:魔王残片
