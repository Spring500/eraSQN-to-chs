	ENDIF
ENDIF

;インセンス
IF TEQUIP:MASTER:薰香
	LOCAL:1 = 0
	SELECTCASE GETBITS(TEQUIP:MASTER:薰香, 4, 6)
	CASE 1p4
		LOCAL:1 = 1
	CASE 1p5
		LOCAL:1 = 3
	CASE 1p6
		LOCAL:1 = 6
	ENDSELECT
	LOCALS = %LOCALS% + %TEXTS("薰香", TEQUIP:MASTER:薰香)%({LOCAL:1})
	LOCAL += LOCAL:1
