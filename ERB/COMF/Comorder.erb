	LOCAL += MARK:快乐刻印 * 1
	LOCALS = %LOCALS% + %MARKNAME:1%LV{MARK:快乐刻印}({MARK:快乐刻印 * 1})
ENDIF
IF MARK:淫纹 && TALENT:男性 == 0
	LOCAL += MARK:淫纹 * 1
