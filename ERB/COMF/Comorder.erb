	LOCALS = %LOCALS%魔王のかけら(-5)
ELSE
	LOCALS = %LOCALS%夢魔でない(0)
ENDIF

;ABL:服从
IF ABL:服从
	LOCAL += ABL:服从 * 4
	LOCALS = %LOCALS% + %ABLNAME:0%LV{ABL:服从}({ABL:服从 * 4})
ENDIF
;ABL:欲望
IF ABL:欲望
	LOCAL += ABL:欲望 * 1
	LOCALS = %LOCALS% + %ABLNAME:1%LV{ABL:欲望}({ABL:欲望 * 1})
ENDIF

