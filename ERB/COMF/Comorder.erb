	LOCAL += 10
	LOCALS = %LOCALS% + %TALENTNAME:75%(10)
ENDIF

;淫乱
IF TALENT:淫乱
	LOCAL += 10
	LOCALS = %LOCALS% + %TALENTNAME:76%(10)
ENDIF

;男嫌い
IF TALENT:讨厌男性 && TALENT:PLAYER:男性
