;ARGSにPLAYERの場所、ARGS:1にTARGETの場所
@COM_ORDER_STAIN(ARGS, ARGS:1)
#FUNCTION
#DIM STAIN_TARGET
#DIM STAIN_PLAYER

;汚れ無視
SIF TALENT:无视污臭
	RETURNF 0

SELECTCASE ARGS
CASE "Ｍ", "手", "Ｐ", "Ｖ", "Ａ", "Ｂ"
	STAIN_PLAYER = STAIN:PLAYER:ARGS
CASEELSE
	RETURNF 0
ENDSELECT

SELECTCASE ARGS:1
CASE "Ｍ", "手", "Ｐ", "Ｖ", "Ａ", "Ｂ"
	STAIN_TARGET = STAIN:(ARGS:1)
CASEELSE
	STAIN_TARGET = 0
ENDSELECT

VARSET LOCAL
;愛液の汚れ(レズっ気で相殺)
IF CHECK_STAIN(STAIN_PLAYER, "Ｖ")
	LOCAL:0 = 17 - ABL:百合倾向*4
	SIF TALENT:男性
		LOCAL:0 -= 6
	SIF TALENT:弱气味
		LOCAL:0 -= 2
	SIF COND("夢魔")
		LOCAL:0 -= 5
	;場所自体がＶでお風呂上りならペナ軽減
	SIF LOCAL:0 > 0 && ARGS == "Ｖ" && TEQUIP:PLAYER:结束淋浴
		LOCAL:0 /= 2
ENDIF
;ペニスの汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｐ")
	LOCAL:1 = 5
	SIF TALENT:讨厌男性 && TALENT:PLAYER:男性
		LOCAL:1 += 10
	SIF TALENT:弱气味
		LOCAL:1 -= 2
	;場所自体がＰでお風呂上りならペナ軽減
	SIF LOCAL:1 > 0 && ARGS == "Ｐ" && TEQUIP:PLAYER:结束淋浴
		LOCAL:1 /= 2
	SIF CALCF("ペニス好き度", TARGET) > 0
		LOCAL:1 -= CALCF("ペニス好き度", TARGET)
ENDIF
;精液の汚れ(夢魔、精液中毒で相殺)
IF CHECK_STAIN(STAIN_PLAYER, "精液")
	SIF ABL:精液上瘾
		LOCAL:2 = 6 - ABL:精液上瘾*2 - (TALENT:弱气味 > 0)
	SIF COND("夢魔") || TALENT:吸精体质
		LOCAL:2 -= 6
ENDIF
;アナルの汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ａ")
	LOCAL:3 = 65 - ABL:受虐倾向*5 - ABL:施虐倾向*2
	SIF COND("夢魔")
		LOCAL:3 -= 10
	SIF TALENT:尻穴狂
		LOCAL:3 -= 20
	SIF TALENT:抖Ｍ
		LOCAL:3 -= 10
	SIF TALENT:抖Ｓ
		LOCAL:3 -= 10
	SIF TALENT:弱气味 && TALENT:恋慕
		LOCAL:3 /= 2
	;場所自体がＡでお風呂上りならペナ軽減
	SIF LOCAL:3 > 0 && ARGS == "Ａ" && TEQUIP:PLAYER:结束淋浴
		LOCAL:3 /= 2
ENDIF
;母乳の汚れ
IF CHECK_STAIN(STAIN_PLAYER, "乳")
	LOCAL:4 += 1
	;幼稚で相殺
	SIF TALENT:幼儿／幼儿退行 || TALENT:幼稚
		LOCAL:4 -= 4
ENDIF
;尿の汚れ
IF CHECK_STAIN(STAIN_PLAYER, "尿")
	LOCAL:5 += 15
	SIF TALENT:弱气味 && TALENT:恋慕
		LOCAL:5 /= 2
	SIF COND("夢魔")
		LOCAL:5 -= 7
ENDIF
;触手の汚れ
IF CHECK_STAIN(STAIN_PLAYER, "触手")
	LOCAL:6 += 20
	SIF TALENT:弱气味 && TALENT:恋慕
		LOCAL:6 /= 2
	;触手経験,旧支配者の名残
	SELECTCASE EXP:触手经验 + TALENT:旧支配者的末裔*500
	CASE IS >= 500
		LOCAL:6 -= 20
	CASE IS >= 300
		LOCAL:6 -= 15
	CASE IS >= 100
		LOCAL:6 -= 10
	CASE IS >= 20
		LOCAL:6 -= 5
	ENDSELECT
ENDIF
;アソコの恥垢汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｖ恥垢")
	LOCAL:20 = 20
	SIF TALENT:弱气味 && TALENT:恋慕
		LOCAL:20 /= 2
	;場所自体がＶでお風呂上りならペナ軽減
	SIF LOCAL:20 > 0 && ARGS == "Ｖ" && TEQUIP:PLAYER:结束淋浴
		LOCAL:20 /= 2
ENDIF
;ちんかすの汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｐ恥垢")
	LOCAL:21 = 20
	SIF TALENT:讨厌男性 && TALENT:PLAYER:男性
		LOCAL:21 += 10
	SIF TALENT:弱气味 && TALENT:恋慕
		LOCAL:21 /= 2
	;場所自体がＰでお風呂上りならペナ軽減
	SIF LOCAL:21 > 0 && ARGS == "Ｐ" && TEQUIP:PLAYER:结束淋浴
		LOCAL:21 /= 2
	IF CALCF("ペニス好き度", TARGET) > 0
		LOCAL:21 /= CALCF("ペニス好き度", TARGET)
		SIF TALENT:献身
			LOCAL:21 -= 5
		SIF TALENT:性变态
			LOCAL:21 -= 5
	ENDIF
ENDIF


;もともとある汚れは無視する＆場所によっては汚れの強さＵＰ
SELECTCASE ARGS:1
CASE "Ｍ"
	LOCAL:3 = MIN(MULTIPLY(LOCAL:3, 150), LOCAL:3 + 20)
CASE "Ｐ"
	LOCAL:0 = 0
	LOCAL:1 = 0
	LOCAL:2 = 0
	LOCAL:5 = 0
CASE "Ｖ"
	LOCAL:0 = 0
	LOCAL:1 = 0
	LOCAL:3 = MIN(MULTIPLY(LOCAL:3, 200), LOCAL:3 + 50)
CASE "Ａ"
	LOCAL:0 = 0
	LOCAL:1 = 0
	LOCAL:3 = 0
CASE "尿道"
	LOCAL:5 = 0
	LOCAL:3 = MIN(MULTIPLY(LOCAL:3, 200), LOCAL:3 + 50)
ENDSELECT

LOCAL = LOCAL:0 + LOCAL:1 + LOCAL:2 + LOCAL:3 + LOCAL:4 + LOCAL:5 + LOCAL:6 + LOCAL:20 + LOCAL:21

SIF TALENT:污臭钝感
	LOCAL = MAX(DIVIDE(LOCAL, 200), LOCAL - 20)
SIF TALENT:污臭敏感
	LOCAL = MIN(MULTIPLY(LOCAL, 200), LOCAL + 20)

LOCAL = MAX(LOCAL, 0)

RETURNF LOCAL
