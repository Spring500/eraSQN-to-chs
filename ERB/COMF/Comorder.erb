		LOCALS = %LOCALS% + 相性良好(15)
	CASE IS >= 200
		LOCAL += 30
		LOCALS = %LOCALS% + 相性最高(30)
	ENDSELECT
ENDIF

SIF ARG == 0
	PRINTFORM %LOCALS%

