		;ABL:百合倾向
		IF ABL:百合倾向
			LOCAL += ABL:百合倾向*2
			LOCALS = %LOCALS% + %ABLNAME:9%LV{ABL:百合倾向}({ABL:百合倾向*2})
		ELSE
			LOCAL -= 10
			LOCALS = %LOCALS% - レズっ気無し(10)
		ENDIF
	ELSEIF TALENT:PLAYER:男性 && TALENT:男性
		;ABL:ＢＬ倾向
		IF ABL:ＢＬ倾向
			LOCAL += ABL:ＢＬ倾向*2
			LOCALS = %LOCALS% + %ABLNAME:13%LV{ABL:ＢＬ倾向}({ABL:ＢＬ倾向*2})
		ELSE
			LOCAL -= 10
			LOCALS = %LOCALS% - ＢＬっ気無し(10)
		ENDIF
	ENDIF
