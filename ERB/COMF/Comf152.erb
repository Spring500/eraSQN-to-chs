IF RESULT == 100 && ARGS != "強制挿入"
	TEQUIP:ARG:尿道バイブ = 0
	TFLAG:14 = 1
	RETURN 0
ELSEIF RESULT == 1 && LOCAL:1
	TEQUIP:ARG:尿道バイブ = 1p1
	B = (CALCF("尿道径") - 3)
ELSEIF RESULT == 2 && LOCAL:2
