@COM37
;オナホール
IF TEQUIP:触手
	PRINTL 触手ペニス責め
ELSE
	PRINTL オナホール
ENDIF

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

IF TEQUIP:触手
	STR:0 = 触手ペニス責め
ELSE
	STR:0 = オナホール
ENDIF

RETURN 1

@FLAG_COM37, ARG
;アソコを露出させる
CALL SETFLAG, "ボトムを露出：テキスト無し", TARGET
SIF TEQUIP:触手
	CALL ADD_STAIN, TARGET, "Ｐ", "触手"

@SOURCE_COM37, ARG
VARSET LOCAL

LOCAL:20 += 10
LOCAL:21 += 80

LOCAL:12 = 120
LOCAL:14 = 70
;オトコかふたなり
IF PENIS(TARGET)
	;ABL:Ｃ感覚をみる
	LOCAL:0 = CALCF("Ｃ刺激", 0)
	LOCAL:0 = SOURCE_EXABL(LOCAL:0, 31, PLAYER)
ENDIF

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM37
PRINTFORMW %CALLNAME:TARGET%のペニスをオナホールに挿入し%TEXTR("て激しく", "て巧みに", "、緩急をつけて")%扱き上げた…

@TRAIN_MESSAGE_COM209
IF TEQUIP:オナホール
	TEQUIP:オナホール = 0
	PRINTFORMW 触手は%CALLNAME:TARGET%のペニスを解放した…
ELSE
	TEQUIP:オナホール = 1
	IF RAND:2 == 0
		PRINTFORMW いやらしく肉ヒダをうねらせた触手が%CALLNAME:TARGET%のペニスに巻き付き、しごき始めた…
	ELSE
		PRINTFORMW 半透明の触手が%CALLNAME:TARGET%のペニスを呑み込み、淫猥な音を立てながら扱き始めた…
	ENDIF
ENDIF


@EQUIP_COM17
;オナホール装備中
CALL SOURCE_COM37, 30
CALL FLAG_COM37, 30
RETURN 1

@EQUIP_COM17_2
;オナホール装備中
IF TEQUIP:オナホール
	IF TEQUIP:触手
		PRINTL ＜触手ペニス責め中＞
	ELSE
		PRINTL ＜オナホール装備中＞
	ENDIF
ENDIF

RETURN 1
