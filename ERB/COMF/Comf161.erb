@COM161
;助手を犯させる
CALL PRINT_TRAIN_NAME(SELECTCOM)

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL SHOW_COM_ORDER_COM, 161

;実行できない
SIF TCVAR:28 == 0
	RETURN 0
;-------------------------------------------------
;実行決定
;-------------------------------------------------
CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM161, ARG
IF TALENT:ASSI:处女
	CALL SETFLAG, "処女喪失フラグ", ASSI
	CALL SETFLAG, "処女簒奪フラグ", TARGET
ENDIF
IF TALENT:童贞
	CALL SETFLAG, "童貞狩りフラグ", ASSI
	CALL SETFLAG, "童貞喪失フラグ", TARGET
ENDIF
;対象のＰ⇔助手のＶの汚れが移動
CALL EQ_STAIN, TARGET, ASSI, 2, 3

@SOURCE_COM161, ARG
VARSET LOCAL

LOCAL:20 += 40
LOCAL:21 += 220

LOCAL:13 = 1500
LOCAL:14 = 800

;ABL:侍奉精神をみる
SELECTCASE ABL:侍奉精神
CASE 0
	LOCAL:5 = 400
CASE 1
	LOCAL:4 = 50
	LOCAL:5 = 800
CASE 2
	LOCAL:4 = 100
	LOCAL:5 = 1500
CASE 3
	LOCAL:4 = 200
	LOCAL:5 = 2300
CASE 4
	LOCAL:4 = 400
	LOCAL:5 = 3500
CASEELSE
	LOCAL:4 = 700
	LOCAL:5 = 5000
ENDSELECT

;ABL:技巧をみる
SELECTCASE ABL:技巧
CASE 0
	TIMES LOCAL:4 , 0.50
	TIMES LOCAL:5 , 0.50
CASE 1
	TIMES LOCAL:4 , 0.80
	TIMES LOCAL:5 , 0.80
CASE 2
CASE 3
	TIMES LOCAL:4 , 1.50
	TIMES LOCAL:5 , 1.50
CASE 4
	TIMES LOCAL:4 , 2.50
	TIMES LOCAL:5 , 2.50
CASEELSE
	TIMES LOCAL:4 , 4.00
	TIMES LOCAL:5 , 4.00
ENDSELECT


;オトコかふたなり
IF PENIS(TARGET)
	SWAP ASSI, PLAYER
	;ABL:Ｃ感觉をみる
	LOCAL:0 = CALCF("Ｃ刺激", 3)
	LOCAL:0 = SOURCE_EXABL(LOCAL:0, 33, PLAYER)
	SWAP ASSI, PLAYER
ENDIF

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM161
PRINTFORMW %CALLNAME:TARGET%に%CALLNAME:PLAYER%を抱かせる事にした…


@COM_ORDER_COM161, ARG
VARSET LOCAL
LOCALS = 

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER, ARG

LOCAL = RESULT

;ABL:侍奉精神
IF ABL:侍奉精神
	LOCAL += ABL:侍奉精神 * 4
	LOCALS = %LOCALS% + %ABLNAME:6%LV{ABL:侍奉精神}({ABL:侍奉精神 * 4})
ENDIF
;恥じらい
IF TALENT:羞耻心强
	LOCAL -= 10
	LOCALS = %LOCALS% - %TALENTNAME:35%(10)
ENDIF
;献身的
IF TALENT:献身
	LOCAL += 6
	LOCALS = %LOCALS% + %TALENTNAME:63%(6)
ENDIF
;快感の否定
IF TALENT:否认快感
	LOCAL -= 1
	LOCALS = %LOCALS% - %TALENTNAME:71%(1)
ENDIF

;助手が処女
IF EXP:ASSI:Ｖ经验 == 0
	LOCAL -= 15
	LOCALS = %LOCALS% - %TEXTS("助手の名称")%が処女(15)
ENDIF

;合計を表示(40以上で実行)
CALL PRINT_COM_ORDER_COM, LOCALS, ARG, LOCAL, 40

RETURN RESULT
