@COM12
;足コキ
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM12, ARG

@SOURCE_COM12, ARG
VARSET LOCAL

LOCAL:20 += 30
LOCAL:21 += 100

LOCAL:14 = 500

;ABL:受虐倾向をみる
SELECTCASE ABL:受虐倾向
CASE 0
	LOCAL:5 = 150
CASE 1
	LOCAL:5 = 300
CASE 2
	LOCAL:5 = 600
CASE 3
	LOCAL:5 = 900
CASE 4
	LOCAL:5 = 1500
CASEELSE
	LOCAL:5 = 2200
ENDSELECT
;倒錯的
SIF TALENT:性变态
	TIMES LOCAL:5 , 1.80

;ABL:Ｃ感觉をみる
LOCAL:0 = CALCF("Ｃ刺激", 1)
LOCAL:0 = SOURCE_EXABL(LOCAL:0, 32, PLAYER)

SIF PENIS(TARGET) == 0 && TEQUIP:バイブ
	CALL SOURCE_COM31, 30

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM12
PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%の
IF PENIS(TARGET)
	PRINTFORMW %TEXTS("ペニス：視覚", TARGET)%ペニスを足でシゴいた…
ELSE
	IF TEQUIP:バイブ
		PRINTFORMW アソコを埋めているバイブを足で弄った。
		;バイブを弄られるのに慣れている
		IF CHECK("バイブ")
			PRINTFORMW %CALLNAME:TARGET%は足を引かれるのに合わせて膣の操作でバイブを押し戻し、更なる責めを待ち望んで腰を突き出した…
		ELSE
			PRINTFORMW 体重の乗せられたバイブで想定外の深い場所を抉られた%CALLNAME:TARGET%は、恐怖の滲んだ顔で%CALLNAME:PLAYER%を睨みつけてきた…
		ENDIF
	ELSE
		PRINTFORMW 股間を足で弄った…
	ENDIF
ENDIF



