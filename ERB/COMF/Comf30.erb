@COM30
;ローター

PRINTL ローター

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = ローター

RETURN 1

@FLAG_COM30, ARG

@SOURCE_COM30, ARG
VARSET LOCAL

LOCAL:20 += 10
LOCAL:21 += 80

LOCAL:12 = 120
LOCAL:14 = 70

;ABL:Ｃ感覚をみる
LOCAL:0 = CALCF("Ｃ刺激", 0)

;アソコが露出していないと快Ｃ減
IF !CHECK_CLO("Ｖ露出") && COND("調教中")
	;ズボン等をはいていると快Ｃ激減
	IF CHECK_OUTER("ズボン") || CHECK_PANTIES("レザー")
		TIMES LOCAL:0 , 0.20
		TIMES LOCAL:12 , 0.20
	ELSE
		TIMES LOCAL:0 , 0.60
		TIMES LOCAL:12 , 0.50
	ENDIF
ENDIF

CALL CALC, "調教ソース", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM30
PRINTFORM %CALLNAME:PLAYER%は
;インサート中
IF INSERT("合体中")
	PRINTFORM %TEXTS("合体中", PLAYER)%
ELSEIF !CHECK_CLO("Ｖ露出")
	PRINTFORM %CLO("股間")%の上から、
;ELSE
;	SIF ABL:Ｃ感覚 >= 3 && (MARK:屈服刻印 == 3 || TALENT:恋慕)
;		PRINT 脚を広げて待っている
ENDIF
PRINTFORM %CALLNAME:TARGET%の
IF PENIS(TARGET)
	IF TALENT:淫核 && PALAM:欲情 > 8000
		PRINT 肥大した
	ELSEIF PALAM:快Ｃ > 3000
		PRINT 勃起した
	ENDIF
	PRINT ペニス
ELSE
	IF CALCF("Ｃ発情") >= 3 && RAND:2
		PRINT 肥大した
	ELSEIF CHECK_CLO("Ｖ露出") && TALENT:クリピアス && RAND:2
		PRINT ピアスで飾られた
	ELSEIF CALCF("Ｃ発情") >= 2 && RAND:2
		PRINT ツンと尖った
	ELSEIF CHECK_CLO("Ｖ露出") && COND("陰毛") >= 6 && RAND:2
		PRINT 陰毛に隠れている
	ELSEIF CALCF("Ｃ発情") >= 2 && RAND:2
		PRINT ぷっくりと膨れた
	ELSEIF TALENT:小柄体型
		PRINT 小さな
	ENDIF
	PRINT クリトリス
ENDIF
IF PREVCOM == SELECTCOM
	PRINTW を小刻みに振動するローターで執拗に責め続けた…
ELSE
	PRINTW に震えるローターを押し当てた……
ENDIF
