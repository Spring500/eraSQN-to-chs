	LOCAL -= 5
	LOCALS = %LOCALS% - %ITEMNAME:1%(5)
ENDIF
;Ａバイブ
IF TEQUIP:アナルバイブ && COND("アナルパール") == 0
	LOCAL -= 10
	LOCALS = %LOCALS% - %ITEMNAME:3%(10)
ENDIF
;ビデオ撮影
IF TEQUIP:ビデオカメラ
	LOCAL -= 20
	LOCALS = %LOCALS% - 撮影中(20)
ENDIF
IF TEQUIP:野外プレイ
	LOCAL -= 10
	LOCALS = %LOCALS% - 野外プレイ(10)
ENDIF

;合計を表示(33以上で実行)
CALL PRINT_COM_ORDER_COM, LOCALS, ARG, LOCAL, 33

RETURN RESULT
