SIF TARGET == 0 || COND("温泉旅行可能") == 0
	MENU_SHOP = %MENU_SHOP%灰色/
MENU_SHOP = %MENU_SHOP%[125]旅行の予約(金曜夜)/

SIF FLAG:208
	MENU_SHOP = %MENU_SHOP%灰色/
MENU_SHOP = %MENU_SHOP%[131]お気に入り＆弱点設定/


SIF NUM("所持アイテム") + NUM("所持衣装") == 0
	MENU_SHOP = %MENU_SHOP%灰色/
MENU_SHOP = %MENU_SHOP%[140]アイテム＆衣装一覧/
SIF NUM("売却可能アイテム") == 0
	MENU_SHOP = %MENU_SHOP%灰色/
