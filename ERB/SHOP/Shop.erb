	MENU_SHOP = %MENU_SHOP%改行/[700]%NAME:TARGET%に名前を付ける/


VARSET LOCALS
SPLIT MENU_SHOP, "/", LOCALS


NEXT_LINE = 0

;SHOP画面の表示
FOR LCOUNT, 0, 100
	SELECTCASE LOCALS:LCOUNT
	CASE ""
		BREAK
	CASE "灰色", "水色", "ハートピンク"
		SETCOLOR DEF_COLOR(LOCALS:LCOUNT)
		CONTINUE
	CASE "改行"
		IF NEXT_LINE%3
			NEXT_LINE = 0
			PRINTL
		ENDIF
		CONTINUE
	CASE "強制改行"
		NEXT_LINE = 0
		PRINTL
		CONTINUE
	ENDSELECT

	PRINTFORM %TEXT_LJ(LOCALS:LCOUNT, 27)%

	RESETCOLOR

	NEXT_LINE += 1
	SIF NEXT_LINE%3 == 0
		PRINTL
NEXT
SIF NEXT_LINE%3
	PRINTL

VARSET LOCALS
;日付イベントがあるキャラを示唆する試み
SELECTCASE TEXTS("日付対応イベント")
;日付固有イベントフラグはイベントが設定されていない日に全消去
CASE ""
	FOR LCOUNT, 1, CHARANUM
		CALL DEL_CEVENT_GROUP, "日付", LCOUNT
	NEXT
CASEELSE
	;こっちは主人のフラグは見ない
	FOR LCOUNT, 2, CHARANUM
		SIF CDAYEV(TEXTS("日付対応イベント"), LCOUNT)
			LOCALS = %LOCALS%[{LCOUNT, 2}]%CALLNAME:LCOUNT%と
	NEXT
ENDSELECT

IF LOCALS != ""
	PRINTL
	SETCOLOR DEF_COLOR("黄色")
	PRINTFORML %SUBSTRING(LOCALS, 0, STRLENS(LOCALS) - 2)%不知道为什么躁动不安……
	PRINTFORML 看来今天注定不会太平静
	RESETCOLOR
ENDIF

MEMO_LINECOUNT = LINECOUNT

