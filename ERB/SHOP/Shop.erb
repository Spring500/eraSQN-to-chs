IF CHARANUM >= 3 && COND("一言イベント中") == 0
	ACTOR = RANDF(CHARANUM - 2) + 2

	PRINTL
	;台詞有る場合に、台詞の名前表示をする？
	SIF CONFIG("台詞の名前非表示") == 0
		PRINTFORML - %CALLNAME:ACTOR% -

