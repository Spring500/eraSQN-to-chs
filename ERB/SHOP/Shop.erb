;調教対象切り替え画面の設定を初期化
FLAG:47 = 0
;調教中フラグなどなどOFF
FLAG:99 = 0
;非表示コマンドの表示状態をリセット
CLEARBIT FLAG:210, 21
;キャラ変更可能フラグリセット
