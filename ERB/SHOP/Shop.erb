IF NUM("所持店舗")
	MENU_SHOP = %MENU_SHOP%[160]喫茶店の準備/
ELSE
	SIF NUM("運営可能店舗") == 0
		MENU_SHOP = %MENU_SHOP%灰色/
	MENU_SHOP = %MENU_SHOP%[160]喫茶店をはじめる/
ENDIF

