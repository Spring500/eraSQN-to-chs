LOCALS = %V_CALLNAME()%%CALLNAME_V()%
RETURNF LOCALS

;接尾詞
@CALLNAME_V()
#FUNCTIONS
LOCALS = %CALLNAME:TARGET%

;ロリ
IF TALENT:小柄体型
	;嗜虐傾向
	IF STRCOUNTS(CSTR:82, "嗜虐") && RAND:2 == 0
		LOCALS = %LOCALS%さま
	ELSEIF RAND:3 == 0
		IF TALENT:オトコ && TALENT:男の娘 == 0
			LOCALS = %LOCALS%くん
		ELSE
			LOCALS = %LOCALS%ちゃん
		ENDIF
	ELSEIF RAND:2
		LOCALS = %LOCALS%たん
	ENDIF
;オトナ
ELSEIF TALENT:長身
	;嗜虐傾向
	IF STRCOUNTS(CSTR:82, "嗜虐") && RAND:2 == 0
		LOCALS = %LOCALS%様
	;妊娠中
	ELSEIF TALENT:妊娠 && RAND:2
		IF RAND:2
			LOCALS = %LOCALS%ママ
		ELSE
			LOCALS = %LOCALS%お母さん
		ENDIF
	ELSEIF RAND:2
		LOCALS = %LOCALS%さん
	ENDIF
;お姉さん
ELSE
	;嗜虐傾向
	IF STRCOUNTS(CSTR:82, "嗜虐") && RAND:2 == 0
		LOCALS = %LOCALS%様
	ELSEIF RAND:3 == 0
		LOCALS = %LOCALS%さん
	ELSEIF RAND:2
		LOCALS = %LOCALS%ちゃん
	ENDIF
ENDIF

RETURNF LOCALS

;接頭詞
@V_CALLNAME()
#FUNCTIONS

VARSET LOCALS
SELECTCASE NO:TARGET
CASE 2
	LOCALS = 小悪魔
CASE 3
	IF CFLAG:8 == 2
		LOCALS = 花精
	ELSE
		LOCALS = 樹精
	ENDIF
CASE 4
	IF CFLAG:8 == 3
		LOCALS = 妖精の女王
	ELSE
		LOCALS = 妖精
	ENDIF
CASE 5
	IF CFLAG:8 == 1
		LOCALS = 水精
	ELSE
		LOCALS = 風精
	ENDIF
CASE 6
	IF RAND:2
		LOCALS = 猫娘
	ELSE
		LOCALS = 化け猫
	ENDIF
CASE 7
	IF RAND:2
		LOCALS = 狼女
	ELSE
		LOCALS = 人狼
	ENDIF
CASE 8, 9, 32
	IF NO:TARGET == 8 && CFLAG:8 == 2
		LOCALS = 泣き娘
	ELSE
		IF RAND:2
			LOCALS = 清純系
		ELSE
			LOCALS = 純情系
		ENDIF
	ENDIF
CASE 10
	IF RAND:2
		LOCALS = 蛇女
	ELSE
		LOCALS = 蛇娘
	ENDIF
CASE 11, 38, 39, 40
	IF RAND:2
		LOCALS = 怪鳥
	ELSE
		LOCALS = 凶鳥
	ENDIF
CASE 12
	IF CFLAG:8 == 2
		LOCALS = 火竜
	ELSE
		IF RAND:3 == 0
			LOCALS = トカゲ
		ELSEIF RAND:2
			LOCALS = 蜥蜴人
		ELSE
			LOCALS = 蜥蜴娘
		ENDIF
	ENDIF
CASE 13, 37
	LOCALS = 人魚
CASE 14
	LOCALS = 天女
CASE 15
	IF RAND:2
		LOCALS = 怪物
	ELSE
		LOCALS = 夢魔
	ENDIF
CASE 16
	IF RAND:2
		LOCALS = 女狐
	ELSE
		LOCALS = キツネ
	ENDIF
CASE 17
	IF CFLAG:8 == 1
		LOCALS = 夜の魔物
	ELSE
		LOCALS = 女王様
	ENDIF
CASE 18
	IF RAND:2
		LOCALS = 女郎蜘蛛
	ELSE
		LOCALS = 雌蜘蛛
	ENDIF
CASE 19
	IF TALENT:小柄体型 && RAND:2
		LOCALS = 魔法少女
	ELSE
		LOCALS = 魔法使い
	ENDIF
CASE 20
	IF CFLAG:8 == 2
		LOCALS = 熾天使
	ELSE
		LOCALS = 天使
	ENDIF
CASE 21
	LOCALS = 堕天使
CASE 22
	LOCALS = 忍者
CASE 23
	LOCALS = 白ウサギ
CASE 25
	LOCALS = 吸血鬼
CASE 26
	LOCALS = 竜騎兵
CASE 27
	LOCALS = 占い師
CASE 28
	LOCALS = 戦士
CASE 30
	LOCALS = 一角獣
CASE 31
	LOCALS = 子鬼
CASE 34
	LOCALS = 石竜
CASE 35
	LOCALS = 火竜
CASE 44
	LOCALS = 高級娼婦
CASE 46
	LOCALS = 魔導人形
CASE 47
	LOCALS = 魔女
CASE 52
	LOCALS = 魔王の欠片
CASE 54
	LOCALS = 司書
	SIF CFLAG:8 == 2
		LOCALS = %LOCALS%さん
CASE 58
	LOCALS = 三月兎
CASE 60
	IF CFLAG:8 == 2 && GETBIT(CFLAG:201, 31)
		LOCALS = 女王蜂
	ELSE
		LOCALS = 働き蜂
	ENDIF
CASE 61
	LOCALS = 戦乙女
CASE 95
	LOCAL = COND("母親")
	SELECTCASE LOCAL
	CASE IS > 0
		IF TALENT:男の娘 || TALENT:オトコ == 0
			LOCALS = %CALLNAME:LOCAL%の息子（？）
		ELSE
			LOCALS = %CALLNAME:LOCAL%の息子
		ENDIF
	CASEELSE
		IF TALENT:男の娘 || TALENT:オトコ == 0
			LOCALS = 淫魔の息子（？）
		ELSE
			LOCALS = 淫魔の息子
		ENDIF
	ENDSELECT
CASE 97
	LOCAL = COND("母親")
	SELECTCASE LOCAL
	CASE IS > 0
		LOCALS = %CALLNAME:LOCAL%の娘
	CASEELSE
		LOCALS = 淫魔の娘
	ENDSELECT
CASEELSE
	IF TALENT:上位夢魔 >= 2
		LOCALS = エルダーサキュバス
	ELSEIF TALENT:上位夢魔
		LOCALS = 高級夢魔
	ELSEIF TALENT:夢魔
		LOCALS = 夢魔
	ELSE
		LOCALS = 魔物
	ENDIF
ENDSELECT
RETURNF LOCALS

