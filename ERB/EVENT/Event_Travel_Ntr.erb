;浮気をするかどうかの判定
;ARGは判定対象キャラ（基本的にTARGETのみだと思われるが一応）
;RAND:100 < チョロい度
;↑で浮気する。相思相愛・恋慕であれば欲求不満が溜まっていない限りほぼ浮気はしない（はず）
;--------------------------------------------------------
@CHECK_UNFAITHFUL_TRAVEL(ARG)
#FUNCTION
VARSET LOCAL

SIF CONFIG("温泉旅行での浮気") == 0
	RETURNF 0

;絶対的浮気判定（まず確率以前の浮気判定）
;処女＋A処女は浮気しない
SIF TALENT:处女 && TALENT:Ａ处女
	RETURNF 0
;淫乱
IF TALENT:ARG:淫乱
	;欲求不満度が0
	IF BASE:ARG:欲求不満 <= 0
		RETURNF 0
	ENDIF
;淫乱でない
ELSE
	IF TALENT:ARG:相思相爱
		;欲求不満度が30未満
		SIF BASE:ARG:欲求不満 < 30
			RETURNF 0
	ELSEIF TALENT:ARG:恋慕
		;欲求不満度が20未満
		SIF BASE:ARG:欲求不満 < 20
			RETURNF 0
	ELSE
		;欲求不満度が10未満
		SIF BASE:ARG:欲求不満 < 20
			RETURNF 0
	ENDIF
ENDIF

;チョロい度増加要素
;欲求不満度がベース
LOCAL += BASE:ARG:欲求不満
;欲望レベルを加算
LOCAL += ABL:ARG:欲望
;淫乱は10ポイント
SIF TALENT:ARG:淫乱
	LOCAL += 10
;惚れっぽいは3ポイント
SIF TALENT:ARG:纯情
	LOCAL += 3
;好奇心は3ポイント
SIF TALENT:ARG:好奇心
	LOCAL += 3
;貞操無頓着は5ポイント
SIF TALENT:ARG:不重贞操
	LOCAL += 5
;愚かは3ポイント
SIF TALENT:ARG:糊涂
	LOCAL += 3
;発情期だと5ポイント、淫乱でさらに10ポイント
IF COND("発情期")
	;淫乱
	SIF TALENT:ARG:淫乱
		LOCAL += 10
	LOCAL += 5
ENDIF

;チョロい度減少要素
;相思相愛は10ポイント
SIF TALENT:ARG:相思相爱
	LOCAL -= 10
;恋慕は5ポイント
SIF TALENT:ARG:恋慕
	LOCAL -= 5
;一線越えないは3ポイント
SIF TALENT:ARG:底线明确
	LOCAL -= 3
;貞操観念は3ポイント
SIF TALENT:ARG:看重贞操
