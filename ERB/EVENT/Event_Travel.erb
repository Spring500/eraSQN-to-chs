		EXP:ARG:绝顶经验 += ARG:1
	IF TALENT:ARG:处女 == 0 && ABL:ARG:Ｖ感觉 >= 3 && EXP:ARG:Ｖ性交经验 >= MAX(20, EXP:ARG:Ａ性交经验/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		;MASTERがペニス持ちなら経験多め
		SIF PENIS(MASTER) == 0 || EXP:MASTER:Ｖ插入经验 < MAX(20, EXP:MASTER:Ａ插入经验/4)
			LOCAL /= 3
		EXP:ARG:Ｖ经验 += LOCAL
		EXP:ARG:Ｖ性交经验 += LOCAL
		EXP:ARG:腰技经验 += LOCAL

		SELECTCASE ARG
		CASE TARGET
			IF PENIS(PLAYER)
				EXP:ARG:精液经验 += LOCAL
				CALL SETFLAG, "膣内精液経験", LOCAL/2, ARG
			ENDIF
		CASE PLAYER
			IF PENIS(TARGET)
				EXP:ARG:精液经验 += LOCAL
				CALL SETFLAG, "膣内精液経験", LOCAL/2, ARG, TARGET
			ENDIF
		ENDSELECT

		IF PENIS(MASTER) && EXP:MASTER:Ｖ插入经验 >= MAX(20, EXP:MASTER:Ａ插入经验/4)
			EXP:MASTER:Ｖ插入经验 += LOCAL/2
			EXP:MASTER:腰技经验 += LOCAL/2
			EXP:MASTER:射精经验 += LOCAL/3
		ENDIF
	ENDIF
	IF TALENT:ARG:Ａ处女 == 0 && ABL:ARG:Ａ感觉 >= 3 && EXP:ARG:Ａ性交经验 >= MAX(20, EXP:ARG:Ｖ性交经验/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		;MASTERがペニス持ちなら経験多め
		SIF PENIS(MASTER) == 0 || EXP:MASTER:Ａ插入经验 < MAX(20, EXP:MASTER:Ｖ插入经验/4)
			LOCAL /= 3
		EXP:ARG:Ａ经验 += LOCAL
		EXP:ARG:Ａ性交经验 += LOCAL
		EXP:ARG:腰技经验 += LOCAL

		SELECTCASE ARG
		CASE TARGET
			IF PENIS(PLAYER)
				EXP:ARG:精液经验 += LOCAL
				CALL SETFLAG, "肛内精液経験", LOCAL/2, ARG
			ENDIF
		CASE PLAYER
			IF PENIS(TARGET)
				EXP:ARG:精液经验 += LOCAL
				CALL SETFLAG, "肛内精液経験", LOCAL/2, ARG, TARGET
			ENDIF
		ENDSELECT

		IF PENIS(MASTER) && EXP:MASTER:Ａ插入经验 >= MAX(20, EXP:MASTER:Ｖ插入经验/4)
			EXP:MASTER:Ａ插入经验 += LOCAL/2
			EXP:MASTER:腰技经验 += LOCAL/2
			EXP:MASTER:射精经验 += LOCAL/3
		ENDIF
	ENDIF
	IF PENIS(ARG) && EXP:ARG:Ｖ插入经验 >= MAX(20, EXP:ARG:Ａ插入经验/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		EXP:ARG:Ｖ插入经验 += LOCAL
		EXP:ARG:腰技经验 += LOCAL
		EXP:ARG:射精经验 += LOCAL/3
	ENDIF
	IF PENIS(ARG) && EXP:ARG:Ａ插入经验 >= MAX(20, EXP:ARG:Ｖ插入经验/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		EXP:ARG:Ａ插入经验 += LOCAL
		EXP:ARG:腰技经验 += LOCAL
		EXP:ARG:射精经验 += LOCAL/3
	ENDIF
	SIF ABL:ARG:ＢＬ倾向 >= 3 || TALENT:ARG:男性 == 0
		EXP:ARG:口交经验 += RAND:(1 + ARG:1)
	SIF ABL:ARG:百合倾向 || TALENT:ARG:男性
		EXP:ARG:舔阴经验 += RAND:(1 + ARG:1)
	SIF COND("指の使用", ARG)
		EXP:ARG:手淫经验 += RAND:(1 + ARG:1)
	SIF COND("脚の使用", ARG) && ABL:ARG:足技
		EXP:ARG:足交经验 += RAND:(1 + ARG:1)
	SIF TALENT:ARG:男性 == 0 && TALENT:ARG:贫乳 == 0 && ABL:ARG:魔乳
		EXP:ARG:乳交经验 += RAND:(1 + ARG:1)
	SIF ABL:ARG:施虐倾向 && EXP:ARG:Ａ刺激经验 >= 20
		EXP:ARG:Ａ刺激经验 += RAND:(1 + ARG:1)
ENDSELECT

SIF SELECTCOM < 0
	RETURN 0

SIF ARG != PLAYER
	SWAP TARGET, ARG

CALL CALC, "経験取得", SELECTCOM, ARG:1

SIF ARG != PLAYER
	SWAP TARGET, ARG
