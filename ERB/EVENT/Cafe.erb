;-------------------------------------------------
;喫茶店
;#eraSQ製作スレＩＲＣメンバー合作！ｗ
;-------------------------------------------------
@SHOP_EX
#DIM LCOUNT
;FLAG:400 店舗数
;FLAG:401 制服の情報
;SAVESTR:1 各店舗の名前　デフォはDream Goblet

PRINTL 
PRINTL 懐いた夢魔を店員にして、可愛い制服を売りにした喫茶店を運営します
PRINTL 収入は日の行の朝に一週間分をまとめて得ます
PRINTL 店舗の通路は広いので、ラミア等の身体の大きな夢魔でも問題なく働けますし、
PRINTL 働き難い姿の夢魔には、体形を人型に変える魔法をかけたり、かけなかったりします
PRINTL オトコでも見目麗しい容姿なら働けますが、お客の夢を壊さない用に配慮して、
PRINTL 中性的な容姿の人に限り、同じ服装でウェイトレスとして働いてもらいます
PRINTFORML

VARSET LOCAL
MONEY:8 = 0

;喫茶店が稼動しているとき
IF NUM("運営店舗")
	FOR LCOUNT, 2, CHARANUM
		IF COND("フロア店員", LCOUNT)
			CALL SETFLAG, "売却価格", LCOUNT
			MONEY:8 += CALCF("喫茶店の給金", LCOUNT)
		ENDIF
	NEXT
	LOCAL = (MONEY:8 * 14 * NUM("運営店舗") ) / CHARA_NUM("フロア店員")/1000*1000
	PRINTFORML フロア店員数：{CHARA_NUM("フロア店員")}人　運営店舗数：{NUM("運営店舗")}　予想収入:%MONEY_C(LOCAL)%/週
ELSEIF NUM("所持店舗")
	PRINTFORML フロア店員数：{CHARA_NUM("フロア店員")}人…店員不足です
ELSE
	PRINTFORM 店員数：{CHARA_NUM("バイト可能")}人　運営可能店舗数：{NUM("運営可能店舗")}　
	FLAG:400 += 1
	IF CHARA_NUM("フロア店員") > 0
		FOR LCOUNT, 2, CHARANUM
			IF COND("フロア店員", LCOUNT)
				CALL SETFLAG, "売却価格", LCOUNT
				MONEY:8 += CALCF("喫茶店の給金", LCOUNT)
			ENDIF
		NEXT
		LOCAL = (MONEY:8 * 14 * NUM("運営店舗") ) / CHARA_NUM("フロア店員")/1000*1000
	ENDIF
	FLAG:400 -= 1
	PRINTFORML 予想収入:%MONEY_C(LOCAL)%/週
ENDIF
PRINTL 

SIF NUM("所持店舗") < NUM("運営可能店舗")
	PRINTFORML  [ 0] 新店を作る　（$10,000） 所持金:%MONEY_C(MONEY)%
IF NUM("所持店舗")
	PRINTFORML  [ 1] 店名を変更　　　　現在：%NAMES("喫茶店")%
	PRINTFORML  [ 2] 制服を変更　　　　現在：%TEXTS("制服")%
	;営業方針はとりあえず凍結
	;PRINTFORML  [ 3] 営業方針を変更　　現在：%TEXTS("営業方針")%
	PRINTFORML  [98] イベント頻度変更　現在：\@ CONFIG("喫茶店描写") ? 時間経過時に発生 # 発生しない \@
ENDIF
SIF CHARA_NUM("バイト可能")
	PRINTL  [99] 店員一覧＆指示
PRINTL [100] 戻る

$INPUT_LOOP
INPUT

IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 98 && FLAG:400
	CALL SETCONFIG, "喫茶店"
ELSEIF RESULT == 99 && CHARA_NUM("バイト可能")
	CALL CAFE_LIST, 0, 0
ELSEIF RESULT == 0 && NUM("所持店舗") < NUM("運営可能店舗")
	IF MONEY >= 10000
		IF NUM("所持店舗") == 0
			SAVESTR:1 = Dream Goblet
			FLAG:401 = 1p0
			PRINTFORML 初期設定では、店名は%NAMES("喫茶店")%で制服は%TEXTS("制服")%となっています
			PRINTFORMW お好みの内容に変更してお楽しみください
		ELSE
			PRINTFORMW 店舗を一つ増やしました
		ENDIF
		MONEY -= 10000
		FLAG:400 += 1
	ELSE
		PRINTFORMW お金が足りません。
	ENDIF
ELSEIF RESULT == 1 && NUM("所持店舗")
	PRINTFORML 喫茶店の名前を『%NAMES("喫茶店")%』から変更します
	PRINTFORML 名前を入力してください（何も入力しない場合は変更しません）
	CALL INPUTS_SELECT, 24
	IF RESULTS != "" && SAVESTR:1 != RESULTS
		SAVESTR:1 = %RESULTS%
		PRINTFORMW 店名を%NAMES("喫茶店")%に変更しました
	ENDIF
ELSEIF RESULT == 2 && NUM("所持店舗")
	CALL SETFLAG, "喫茶店の服装"
ELSE
	GOTO INPUT_LOOP
ENDIF

RESTART

@CAFE
#DIM LCOUNT
#DIM MEMO_TARGET
#DIM MEMO_LINECOUNT, 2

;ここではTARGETを変更させるので保存
MEMO_TARGET = TARGET
TARGET = 0

;同じイベントは起こさない
VARSET TFLAG
;勤労フラグ初期化(一日二回あるので)
FOR LCOUNT, 1, CHARANUM
	CLEARBIT CFLAG:LCOUNT:95, 1
NEXT

IF CONFIG("喫茶店描写")
	PRINTL 
	CUSTOMDRAWLINE ☆

	;挨拶。LINECOUNTを記録して、キャラが喋ったかを判別させる
	MEMO_LINECOUNT = LINECOUNT

	;喋るキャラの選別
	FOR LCOUNT, 1, MAX(4, NUM("運営店舗") + 1)
		;挨拶していない人から選出
		TARGET = FIND_COND("出演可能な店員", "喫茶店挨拶まだ")

		MEMO_LINECOUNT:1 = LINECOUNT
		;台詞有る場合に、台詞の名前表示をする？
		SIF CONFIG("台詞の名前非表示") == 0
			PRINTFORM %TEXT_LJ(CALLNAME:TARGET, 16)%

		CALL SELF_KOJO, "喫茶店"

		;挨拶フラグ
		SETBIT CFLAG:95, 6

		;台詞が無いなら辻褄あわせ
		IF LINECOUNT == MEMO_LINECOUNT:1
			PRINTL 
			CLEARLINE 1
		ENDIF
	NEXT
	IF LINECOUNT > MEMO_LINECOUNT
		WAIT
		PRINTL 
	ENDIF
ENDIF

FOR LCOUNT, 1, NUM("運営店舗") + 1
	;FIND_CONDはTARGET以外から探すので一旦リセット
	TARGET = 0
	;挨拶済みを優遇
	TARGET = FIND_COND("出演可能な店員", "喫茶店挨拶済み")
	SIF TARGET == 0
		TARGET = FIND_COND("出演可能な店員")

	;収入get
	CALL SETFLAG, "売却価格", TARGET
	MONEY:7 += CALCF("喫茶店の給金", TARGET)

	;イベント描写フラグON
	SETBIT CFLAG:95, 1
	IF CONFIG("喫茶店描写")
		SIF LCOUNT >= 2
			PRINTL 

		CALL EVENT_CAFE, LCOUNT
	ENDIF
NEXT

IF CONFIG("喫茶店描写")
	CUSTOMDRAWLINE ☆
	PRINTL 
ENDIF

TARGET = MEMO_TARGET

