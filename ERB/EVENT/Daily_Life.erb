	;SIF (ABL:服从 >= 3 || TALENT:恋慕) && MARK:排斥刻印 == 0
	;	CALL CALC, "ランダム選択", 39
	;授乳場面に遭遇
	SIF BASE:育儿中
		CALL CALC, "ランダム選択", 38
	;羞恥デートイベント（ローター所持、オトコでない非処女かつマゾっ気Lv3以上かつ自慰中毒3LVかつスカートかつ主人サドっ気Lv3以上、足がある人限定）
	SIF ITEM:跳蛋 && EQUIP:裙子 && TALENT:处女 + TALENT:男性 == 0 && MIN(ABL:受虐倾向, ABL:自慰上瘾, ABL:MASTER:施虐倾向) >= 3 && COND("脚が無い") == 0
