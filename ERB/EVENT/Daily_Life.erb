	;禁断の知識の時、妖しい儀式を……
	SIF TALENT:禁忌知识
		CALL CALC, "ランダム選択", 32
	;歌唱技能持ちの時、歌の練習を……
	SIF TALENT:歌唱技能
		CALL CALC, "ランダム選択", 31
	;狂気の時、ぶつぶつ言ってみる
	SIF TALENT:狂气
		CALL CALC, "ランダム選択", 30
	;魔法使いの時、魔法の儀式をorタロット占い
	SIF TALENT:魔法使
		CALL CALC, "ランダム選択", 29
	;調合知識持ちの時、実験をしてみるor原料育成
	SIF TALENT:调合知识
		CALL CALC, "ランダム選択", 28
	;調合知識、大人しいの時本を読ませてみる
	SIF TALENT:腼腆 || TALENT:调合知识
		CALL CALC, "ランダム選択", 27
	;おもらし癖の時、世界地図を広げる(昼)
	SIF COND("昼") && TALENT:吸血鬼 == 0 && TALENT:漏尿癖
		CALL CALC, "ランダム選択", 26
	;日光浴の時、日向ぼっこ(昼)
	SIF COND("昼") && TALENT:日光浴
		CALL CALC, "ランダム選択", 25
	;月光浴の時、月見（夜）
	SIF COND("夜") && TALENT:月光浴
		CALL CALC, "ランダム選択", 24
	;動物耳の時、庭を歩いてみる
	SIF TALENT:兽耳
		CALL CALC, "ランダム選択", 23
	;針さばき持ちのとき、お裁縫
	SIF TALENT:针术
		CALL CALC, "ランダム選択", 22
	;献身的のとき、家事をさせてみる
	SIF TALENT:献身
		CALL CALC, "ランダム選択", 21
	;献身的、治療もちの場合お祈りしてみる
	SIF COND("昼") && TALENT:吸血鬼 == 0 && (TALENT:献身 || TALENT:治愈)
		CALL CALC, "ランダム選択", 20
	;感情乏しいor無関心の時はぼーっとしてみる
	SIF TALENT:冷漠 || TALENT:感情贫乏
		CALL CALC, "ランダム選択", 19
	;目立ちたがりのとき、踊ってみる
	SIF TALENT:喜欢被关注 && MARK:排斥刻印 != 3 && CHARA_NUM("調教経験あり") >= 2
		CALL CALC, "ランダム選択", 18
	;臆病・気丈の時、夜間見回り中に遭遇する
	SIF COND("夜") && (TALENT:胆怯 || TALENT:刚强)
		CALL CALC, "ランダム選択", 17
	;猫舌の時、木に昇ってみるorマタタビワインで酔っ払ってみる
	SIF COND("昼") && TALENT:吸血鬼 == 0 && TALENT:猫舌
		CALL CALC, "ランダム選択", 16
	;反抗的・素直の時、ちょっとした用事を頼まれてみる
	SIF TALENT:叛逆 || TALENT:坦率
		CALL CALC, "ランダム選択", 15
	;妖精の時、花壇で遊んでみる
	SIF COND("昼") && TALENT:吸血鬼 == 0 && TALENT:妖精
		CALL CALC, "ランダム選択", 14
	;とったビデオを見る…変数P条件にして簡易にやっちゃうかな…恥じらいの時
