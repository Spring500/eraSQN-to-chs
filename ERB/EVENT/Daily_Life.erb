@DAILY_LIFE
#DIM LCOUNT
#DIM MEMO_TARGET

;書類仕事して無かったら起きるかも
IF FLAG:12 == 0 && RAND:10 == 0 && CHARA_NUM("書類仕事")
	CALL EVENT_DESKWORK
	RETURN 1
ENDIF

;キャラ登録数が2以下or調教を行ってない時は引き返す
SIF CHARANUM <= MASTER + 1 || CHARA_NUM("調教経験あり") == 0 || COND("同衾イベント", MASTER)
	RETURN 1

MEMO_TARGET = TARGET

FOR LCOUNT, 0, MIN(1 + CHARA_NUM("日常イベント出演可能")/10, 3)
	TARGET = MEMO_TARGET
	CALL DAILY_LIFE_MAIN
NEXT

TARGET = MEMO_TARGET

@DAILY_LIFE_MAIN
#DIM LCOUNT
#DIM GUEST
#DIM GUEST2
#DIM GUEST3
#DIM MEMO_LINECOUNT
;該当するキャラの人数
#DIM NUM_FIND
;イベントが起きる人
#DIM ACTOR

DRAWLINE

$REPLAY0

;まず、メインキャラを選ぶ
ACTOR = 0
NUM_FIND = 0

FOR LCOUNT, 2, CHARANUM
	SIF EXP:LCOUNT:调教次数 == 0 || COND("日常イベント出演済み", LCOUNT)
		CONTINUE

	NUM_FIND += 1
	SIF RAND:NUM_FIND == 0
		ACTOR = LCOUNT
NEXT

SIF ACTOR == 0
	RETURN 0

;PRINTFORML ACTOR = [{ACTOR, 2}]%CALLNAME:ACTOR%, TARGET = [{TARGET, 2}]%CALLNAME:TARGET%

;TARGETとASSIの選ばれる確率を優遇してみる
IF RAND:3 == 0 && EXP:调教次数 && COND("日常イベント出演済み") == 0
	ACTOR = TARGET
ELSEIF ASSI && RAND:2 && EXP:ASSI:调教次数 && COND("日常イベント出演済み", ASSI) == 0
	ACTOR = ASSI
ENDIF

TARGET = ACTOR

;日常イベント出演済みフラグON
SETBIT CFLAG:95, 4

;"調教する"を選択された人数を参照する時には、CHARA_NUM("調教経験あり")で分岐させてください。
;例）調教に呼び出した夢魔が3人以上いないとダメ、という条件なら、 CHARA_NUM("調教経験あり") >= 3 を用いる。

MEMO_LINECOUNT = LINECOUNT
;キャラ登録番号ごとに口上を呼び出す
CALL DAILY_LIFE_MESSAGE
;イベント発生したならここで終了
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

MEMO_LINECOUNT = LINECOUNT
;寝取られイベント
CALL EVENT_DRAGON_NTR, "日常"
;イベント発生したならここで終了
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;出演者用フラグ
VARSET GUEST
VARSET GUEST2
VARSET GUEST3

;昼(TIME == 0)と夜(TIME == 1)で分岐することが多い
;体力が低め（MAXの半分or1000の低い方以下）の時は休ませる
IF BASE:体力 <= MIN(MAXBASE:体力/2, 1000)
	;昼の吸血鬼
	IF COND("昼") && TALENT:吸血鬼
		PRINTFORMW 通りすがりにふと%CALLNAME:TARGET%の部屋を覗いてみた……
		PRINTFORMW カーテンの締めきられた真っ暗な部屋に、蓋のきっちり閉じられた棺桶が置いてある
		PRINTFORMW 恐らく棺桶の中で寝ているのだろう……
		RETURN 1
	;昼の日光浴
	ELSEIF COND("昼") && TALENT:日光浴
		;動物耳の時・マーメイドは…多分ないと思う
		IF TALENT:兽耳
			PRINTFORMW 通りすがりに、庭の日のあたる場所で%CALLNAME:TARGET%を見つけた……
			PRINTFORMW %CALLNAME:TARGET%は毛布を敷いてその上で丸くなって眠っている
		ELSE
			PRINTFORMW 通りすがりに、庭の日のあたる場所で眠っている%CALLNAME:TARGET%を見つけた……
			PRINTFORMW %CALLNAME:TARGET%は木にかけたハンモックに毛布を敷いて寝ている
		ENDIF
	;夜の月光浴
	ELSEIF COND("夜") && TALENT:月光浴
		PRINTFORMW ふと窓の外から見える屋根のてっぺんに、%CALLNAME:TARGET%がいるのが見えた
		PRINTFORMW 月を見上げている……
		IF MARK:排斥刻印 == 3
			PRINTFORMW こちらに気付いて、姿を隠した……
		ELSEIF MARK:屈服刻印 >= 2 && RAND:2 == 0
			PRINTFORMW こちらに気付いて、微笑んでみせた……
		ELSE
			PRINTFORMW こちらに気付いて、妖しい笑みを浮かべた……
		ENDIF
		RETURN 1
	;動物耳の時
	ELSEIF TALENT:兽耳
		PRINTFORMW 通りすがりにふと%CALLNAME:TARGET%の部屋を覗いてみた……
		PRINTFORMW %CALLNAME:TARGET%はベッドの上で丸くなって眠っている
	;マーメイドの時
	ELSEIF TALENT:人鱼
		PRINTFORMW 通りすがりにふと%CALLNAME:TARGET%の部屋を覗いてみた……
		PRINTFORMW %CALLNAME:TARGET%は水槽の中で漂うようにして眠っている
	;その他
	ELSE
		PRINTFORMW 通りすがりにふと%CALLNAME:TARGET%の部屋を覗いてみた……
		PRINTFORMW %CALLNAME:TARGET%はベッドで眠っているようだ
	ENDIF

	;反発刻印3Lvの時
	IF MARK:排斥刻印 == 3
		;臆病or悲観的のとき
		IF TALENT:胆怯 || TALENT:阴郁
			PRINTFORMW その表情は悲しげだ……
		ELSEIF RAND:2 == 0
			PRINTFORMW その表情は険しい……
		ELSE
			PRINTFORMW その表情は固い……
		ENDIF
	;恋慕の時
	ELSEIF TALENT:恋慕
		IF RAND:8 == 0
			PRINTFORMW 寝乱れた姿が色っぽい……
		;生意気or小悪魔の時
		ELSEIF (TALENT:自大 || TALENT:小恶魔) && RAND:7 == 0
			PRINTFORMW 眠りながらニヤニヤしている……
			PRINTFORMW ……何の夢を見ているんだか……
		;謎の魅力or魅惑の時
		ELSEIF (TALENT:魅惑 || TALENT:魅惑) && RAND:6 == 0
			PRINTFORMW ふっとその寝顔に魅入られてしまうような気がした……
		;幼稚の時
