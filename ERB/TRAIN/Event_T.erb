	PRINTFORML {BASE:PLAYER:絶頂, 6} 絶頂
	DRAWLINE
CASE "ムードハート"
	;ムードをハートの数にする
	LOCAL = MOOD_HEART()

	FOR LCOUNT, 1, 7
		IF LOCAL >= LCOUNT
			SETCOLOR DEF_COLOR("ハートピンク")
		ELSE
			SETCOLOR DEF_COLOR("灰色")
		ENDIF
		CALL HEARTMARK
	NEXT
	IF LOCAL >= 6
		PRINT
		CALL HEARTMARK
		PRINT MAX
		CALL HEARTMARK
	ENDIF
