	CASE IS <= 49
		TALENT:ARG:Ａ名器 = 1
	CASE IS <= 89
		TALENT:ARG:Ａ名器 = 1 + RAND:2
	CASEELSE
		TALENT:ARG:Ａ名器 = 1 + RAND:3
	ENDSELECT

	SELECTCASE RAND:100 + TALENT:ARG:上位梦魔*10
	CASE IS <= 49
		TALENT:ARG:喉名器 = 1
	CASE IS <= 89
		TALENT:ARG:喉名器 = 1 + RAND:2
	CASEELSE
		TALENT:ARG:喉名器 = 1 + RAND:3
	ENDSELECT

	SELECTCASE RAND:100 + TALENT:ARG:上位梦魔*10
	CASE IS <= 49
		TALENT:ARG:尿道名器 = 1
	CASE IS <= 89
		TALENT:ARG:尿道名器 = 1 + RAND:2
