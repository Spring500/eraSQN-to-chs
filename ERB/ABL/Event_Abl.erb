;主に、特殊な技能などの設定関数を置く。
;-------------------------------------------------
@GET_EXABL, ARG
#DIM LCOUNT
SIF ARG <= 0
	RETURN 0

;具体的なＬＶの算出
FOR LCOUNT, 30, 35
	LOCALS = %ABLNAME:LCOUNT%
	SELECTCASE LOCALS
	CASE "舌技"
		LOCAL = (EXP:ARG:口交经验 + EXP:ARG:舔阴经验 + EXP:ARG:亲吻经验)/3
	CASE "指技"
		LOCAL = EXP:ARG:手淫经验
	CASE "足技"
		LOCAL = EXP:ARG:足交经验
	CASE "腰技"
		LOCAL = EXP:ARG:腰技经验
	CASE "魔乳"
		LOCAL = EXP:ARG:乳交经验
	ENDSELECT
	IF ARG == MASTER
		TIMES LOCAL , 0.50
	ELSEIF TALENT:ARG:聪慧
		TIMES LOCAL , 1.50
	ELSEIF TALENT:ARG:糊涂
		TIMES LOCAL , 0.70
	ENDIF
	ABL:ARG:LOCALS = TALENT:ARG:LOCALS + MAX(GET_EXP_LV(LOCAL) - 1, 0)
NEXT

CALL GET_EXABL2, ARG


