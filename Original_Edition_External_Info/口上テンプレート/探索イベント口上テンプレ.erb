;-------------------------------------------------
;探索コマンド口上
;@KOJO_MESSAGE_SEXBATTLE_A_X1は出現時の挨拶
;@KOJO_MESSAGE_SEXBATTLE_B_X1は決着時の台詞
;@KOJO_MESSAGE_SEXBATTLE_C_X1は着いてきた時の台詞
;X1をキャラ番号に置換し、基本的に一行で、PRINTFORML を用いること。
;-------------------------------------------------
@KOJO_MESSAGE_SEXBATTLE_A_X1

@KOJO_MESSAGE_SEXBATTLE_B_X1
;夢魔がイかされた
IF BASE:6 <= 0
	;両穴征服
	IF F:4 & 1 && F:4 & 2
	;Ｖピストン
	ELSEIF F:4 & 1
	;Ａピストン
	ELSEIF F:4 & 2
	;エースオブスペード
	ELSEIF TFLAG:16 == 10
	;ヴァッジ
	ELSEIF TFLAG:16 == 4
		;クンニ
		IF R == 2
		;指で愛撫
		ELSE
		ENDIF
	;アナル
	ELSEIF TFLAG:16 == 3
		;アナル舐め
		IF R == 2
		;指で愛撫
		ELSE
		ENDIF
	;バスト
	ELSEIF TFLAG:16 == 2
		;乳首舐め
		IF R == 2
		;胸揉み
		ELSE
		ENDIF
	;キス
	ELSEIF TFLAG:16 <= 1
	;ここは上が全部書かれていると喋らないが、分岐のどれかをコメントアウトさせた時などに使用する
	ELSE

	ENDIF
;探索側をイかせた
ELSE
	;中出し
	IF F:3 == 1

	;Ａ中出し
	ELSEIF F:3 == 2

	;エースオブスペード
	ELSEIF F:3 == 3

	;キス
	ELSEIF TFLAG:17 == 1

	;パイズリ
	ELSEIF TFLAG:17 == 2

	;アナル責め
	ELSEIF TFLAG:17 == 3

	;顔面騎乗
	ELSEIF TFLAG:17 == 4

	;手コキ
	ELSEIF TFLAG:17 == 5

	;フェラ
	ELSEIF TFLAG:17 == 6

	;足コキ
	ELSEIF TFLAG:17 == 7

	;シックスナイン
	ELSEIF TFLAG:17 == 8

	;ここは上が全部書かれていると喋らないが、他の分岐のどれかをコメントアウトさせた時などに使用する
	ELSE

	ENDIF
ENDIF

@KOJO_MESSAGE_SEXBATTLE_C_X1

